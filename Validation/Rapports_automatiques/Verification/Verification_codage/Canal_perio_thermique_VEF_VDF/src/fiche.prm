Parameters {
	Title "Periodic channel with thermic balance - VDF - VEF"
	Description "Validation of periodic compensation source term for Convection_Diffusion_std equation. Both for VDF VEF."

	Author "A.Bruneton"
    TestCase   VEF_ref   canal_perio_VEF_ref.data
    TestCase   VEF       canal_perio_VEF.data
    TestCase   VDF_ref   canal_perio_VDF_ref.data
    TestCase   VDF       canal_perio_VDF.data
}


Chapter {
	Title "Comparison mean temperature, max temperature"
	Description "Check"  
	
	Figure {
        Title "Comparison"
        Description ""  
        Dimension 2
        include_description_curves 0
        LabelX "T(s)"
        LabelY "Temp"

        Curve {
              legend "VEF_ref"
              file VEF_ref/canal_perio_VEF_ref_TEMPERATURE_MOYENNE.son
              Style linespoints
          }
        Curve {
              legend "VDF_ref"
              file VDF_ref/canal_perio_VDF_ref_TEMPERATURE_MOYENNE.son
              Style linespoints
          }

        Curve {
              legend "VEF"
              file VEF/canal_perio_VEF_TEMPERATURE_MOYENNE.son
              Style linespoints
          }

        Curve {
              legend "VDF"
              file VDF/canal_perio_VDF_TEMPERATURE_MOYENNE.son
              Style linespoints
          }
    }
	
} # Chapter
	


