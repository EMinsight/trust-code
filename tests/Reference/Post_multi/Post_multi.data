# Thermohydraulique 2D : Couplage Boussinesq et schema quick #
# PARALLEL OK #
dimension 2
Pb_Thermohydraulique pb
Domaine dom
# BEGIN MESH #
Mailler dom
{
	Pave Cavite 
		{
		Origine 0. 0.
     		Nombre_de_Noeuds 11 11
		Longueurs 0.01 0.01
		}
		{
		Bord Gauche X = 0.  0. <= Y <= 0.01
		Bord Haut Y = 0.01  0. <= X <= 0.01
		Bord Bas Y = 0.     0. <= X <= 0.01
		Bord Droit X = 0.01 0. <= Y <= 0.01
		}
}
Trianguler dom
VerifierCoin dom { }
# END MESH #
# BEGIN PARTITION
Partition dom
{
	Partition_tool tranche { tranches 2 1 }
	Larg_joint 2
	zones_name DOM
}
End
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #

VEFPreP1B dis

Scheme_euler_explicit sch
Read sch
{
	nb_pas_dt_max 100
	tinit 0.
	tmax 1.2
	dt_min 0.0001
	dt_max 0.01
	dt_impr 0.01
	dt_start dt_min
	dt_sauv 100
	seuil_statio 1.e-8
}

Fluide_Incompressible fluide
Read fluide
{
	mu Champ_Uniforme 1 1.85e-5
	rho Champ_Uniforme 1 1.17
	lambda Champ_Uniforme 1 0.0262 
	Cp Champ_Uniforme 1 1006
	beta_th Champ_Uniforme 1 3.41e-3
}

Champ_Uniforme gravite
Read gravite 2 0 -9.81

Associate fluide gravite
Associate pb dom
Associate pb sch
Associate pb fluide

Discretize pb dis

Read pb
{
	Navier_Stokes_standard
	{
		solveur_pression GCP { 
			precond ssor { omega 1.6 }
			seuil 1.000000e-14 
			impr 
		}
		convection { muscl }
		diffusion { }
		sources { boussinesq_temperature { T0 5. } }
		initial_conditions {
			vitesse Champ_Uniforme 2 0. 0.
			}
		boundary_conditions {
			Haut paroi_fixe
			Bas paroi_fixe
			Gauche paroi_fixe
			Droit paroi_fixe
		}
	}
	Convection_Diffusion_Temperature
	{
		diffusion { }
		convection { muscl }
		boundary_conditions 
		{
			Haut paroi_adiabatique
			Bas paroi_adiabatique
 			Gauche paroi_temperature_imposee Champ_Front_Uniforme 1 10.
			Droit  paroi_temperature_imposee Champ_Front_Uniforme 1 0. 
		}
		initial_conditions { Temperature Champ_Uniforme 1 5. }
	}

	Post_processings
	{
		lml
		{
			format lml
			fichier Post_multi
			Probes
			{
				sonde_V vitesse periode 0.01 segment 10 0.0005 0.005 0.0095 0.005
			}
			fields dt_post 0.01
			{
				vitesse elem
				temperature elem
			}
		}
		lata
		{
			format lata
			fichier Post_multi
			fields dt_post 0.001
			{
				vitesse faces
				vitesse elem
				temperature elem	
				temperature faces
			}			
		}
		med
		{
			format med
			fichier Post_multi
			fields dt_post 0.001
			{
				vitesse elem
				temperature elem
			}			
		}
	}
}

Solve pb
End

