            colonnes 1 2
            style linespoints
        }
        Curve
        {
            fit_curve a*x**b a,b
            legend sprintf("triangle/VEFPreP1B order %3.1f", param1)
            file triangle/VEFPreP1B/conv_glob_v
            colonnes 1 2
            style linespoints
        }
    }
    Figure
    {
        labelx "Ne^{-1/2}"
        labely erreur L2
        logx
        logy
        Include_Description_Curves 0
        legend below title "Legend"
        width 15cm
        Title "Ordres de convergence - p"
        Curve
        {
            legend cartesian/CoviMAC
            file cartesian/CoviMAC/conv_glob_p
            colonnes 1 2
            style linespoints
        }
        Curve
        {
            legend cartesian/PolyMAC
            file cartesian/PolyMAC/conv_glob_p
            colonnes 1 2
            style linespoints
        }
        Curve
        {
            legend cartesian/VDF
            file cartesian/VDF/conv_glob_p
            colonnes 1 2
            style linespoints
        }
        Curve
        {
            legend non_conformal/CoviMAC
            file non_conformal/CoviMAC/conv_glob_p
            colonnes 1 2
            style linespoints
        }
        Curve
        {
            legend non_conformal/PolyMAC
            file non_conformal/PolyMAC/conv_glob_p
            colonnes 1 2
            style linespoints
        }
        Curve
        {
            legend triangle/CoviMAC
