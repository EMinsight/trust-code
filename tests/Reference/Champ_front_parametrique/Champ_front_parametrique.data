# Teste Champ_front_parametrique #
# PARALLEL NOT #
dimension 2
Pb_hydraulique pb
Domaine dom
# BEGIN MESH #
Read_file upwind.geo ;

# END MESH #
# BEGIN PARTITION
Postraiter_domaine
{
    fichier mesh.lata
    format lata
    domaine dom
}

Partition dom
{
    Partition_tool tranche { tranches 2 1 }
    Larg_joint 1
    zones_name DOM
}
End
END PARTITION #

# BEGIN SCATTER
Scatter ../upwind/DOM.Zones dom
END SCATTER #

VDF dis

Scheme_euler_explicit sch
Read sch
{
    tinit 0
    dt_impr 5.e-1
    seuil_statio 1.e-2
}

Associate pb dom
Associate pb sch
Discretize pb dis

Read pb
{

    fluide_incompressible {
        mu Champ_Uniforme 1 3.7e-05
        rho Champ_Uniforme 1 2
    }
    Navier_Stokes_standard
    {
        solveur_pression petsc cholesky { }
        convection { amont }
        diffusion {  }
        initial_conditions {
            vitesse Champ_Uniforme 2 0. 0.
        }
        boundary_conditions {
            Obstacle paroi_fixe
            Symetrie symetrie
            Sortie frontiere_ouverte_pression_imposee Champ_front_Uniforme 1 0.
            Entree frontiere_ouverte_vitesse_imposee champ_front_parametrique { probleme pb fichier velocity.txt }
        }
    }

    Post_processing
    {
        Probes
        {
            sonde_pression pression periode 0.005 points 2 0.13 0.105 0.13 0.115
            sonde_vitesse_entree vitesse periode 0.005 point 1 0.0 0.11
            sonde_vitesse_centre vitesse periode 0.005 point 1 0.13 0.105
        }

        Format lata
        fields dt_post 1
        {
            pression elem
            vitesse elem
        }
    }
}
Solve pb
End

