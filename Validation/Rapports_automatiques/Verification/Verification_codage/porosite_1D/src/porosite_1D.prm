Parameters
{
    Title "Ecoulement avec saut de porosite en 1D"
    Description "Ecoulement unidirectionel, sans diffusion. Le domaine est 1D. On compare VDF, PolyMAC, CoviMAC Ã  la solution qui satisfait la conservation de la charge."
    Author "Y.G."
    TestCase VDF "jdd.data"
    TestCase PolyMAC "jdd.data"
    TestCase CoviMAC "jdd.data"
}

Chapter
{
    Title "Residuals"
    Description ""

    Figure
    {
        title_figure residus
        include_description_curves 0
        labelX "t (s)"
        labelY "residuals"
        logY

        width 10cm
        grid
        legend below title "Legende"
        Curve
        {
            legend VDF
            file ./VDF/jdd.dt_ev
            columns 1 4
            style lines
            Linewidth 2
        }
        Curve
        {
            legend PolyMAC
            file ./PolyMAC/jdd.dt_ev
            columns 1 4
            style lines
            Linewidth 2
        }
        Curve
        {
            legend CoviMAC
            file ./CoviMAC/jdd.dt_ev
            columns 1 4
            style lines
            Linewidth 2
        }
    }

    Figure
    {
        title_figure vitesse
        include_description_curves 0
        labelX "z (m)"
        labelY "v"

        width 7.5cm
        grid
        legend below title "Legende"
        nb_img_without_newline 2
        Curve
        {
            legend VDF
            segment ./VDF/jdd V
            columns 1 3
            style lines
            Linewidth 2
        }
        Curve
        {
            legend PolyMAC
            segment ./PolyMAC/jdd V
            columns 1 3
            style lines
            Linewidth 2
        }
        Curve
        {
            legend CoviMAC
            segment ./CoviMAC/jdd V
            columns 1 3
            style lines
            Linewidth 2
        }
        Curve
        {
            legend Analytique
            segment ./VDF/jdd V_ex
            columns 1 3
            style points
        }
    }

    Figure
    {
        title_figure pression
        include_description_curves 0
        labelX "z (m)"
        labelY "v"

        width 7.5cm
        grid
        legend below title "Legende"
        nb_img_without_newline 2
        Curve
        {
            legend VDF
            segment ./VDF/jdd P
            columns 1 2
            style lines
            Linewidth 2
        }
        Curve
        {
            legend PolyMAC
            segment ./PolyMAC/jdd P
            columns 1 2
            style lines
            Linewidth 2
        }
        Curve
        {
            legend CoviMAC
            segment ./CoviMAC/jdd P
            columns 1 2
            style lines
            Linewidth 2
        }
        Curve
        {
            legend Analytique
            segment ./VDF/jdd P_ex
            columns 1 2
            style points
        }
    }
}
