# Hydraulique 2D VEF : Schema EF_stab avec sous zone #
# pour degenerer en amont en sortie  #
# PARALLEL OK #

dimension 2
Pb_hydraulique pb
Domaine dom
# BEGIN MESH #
Mailler dom
{
    Pave Cavite
    {
        Origine 0. 0.
        Nombre_de_Noeuds 11 11
        Longueurs 1. 1.
    }
    {
        Bord Gauche X = 0.	0. <= Y <= 1.
        Bord Haut   Y = 1.	0. <= X <= 1.
        Bord Bas    Y = 0.	0. <= X <= 1.
        Bord Droit  X = 1.	0. <= Y <= 1.
    }
}
Trianguler_H dom

Sous_zone ssz_amont
Associate ssz_amont dom
Read ssz_amont
{
    Rectangle
    Origine 0.7 0.
    Cotes 0.3 1
}
# END MESH #

# BEGIN PARTITION
Partition dom
{
    Partition_tool metis { Nb_parts 2 }
    Larg_joint 2
    zones_name DOM
}
End
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
Sous_Zone ssz_amont
Associate ssz_amont dom
Read ssz_amont { Fichier ssz_amont.ssz }
END SCATTER #

VEFPreP1B dis

Scheme_euler_explicit sch
Read sch
{
    tinit 0.
    tmax 2
    dt_min 0.00001
    dt_max 1.
    dt_impr 0.0001
    dt_sauv 100
    seuil_statio 1.e-8
}

Fluide_Incompressible fluide
Read fluide
{
    mu  Champ_Uniforme 1 1.85e-5
    rho Champ_Uniforme 1 1.17
}

Associate pb dom
Associate pb sch
Associate pb fluide

Discretize pb dis

# Debog pb seq faces 1.e-6 0 #

Read pb
{
    Navier_Stokes_standard
    {
        solveur_pression GCP
        {
            precond ssor { omega 1.5 }
            seuil 1.e-10
            impr
        }
        convection { EF_stab { amont_sous_zone ssz_amont } }
        diffusion { }
        initial_conditions { vitesse Champ_fonc_xyz dom 2 y*(1-y) 0. }
        boundary_conditions
        {
            Haut	paroi_fixe
            Bas	paroi_fixe
            Gauche	frontiere_ouverte_vitesse_imposee  Champ_front_fonc_xyz 2 y*(1-y) 0.
            Droit	frontiere_ouverte_pression_imposee Champ_front_Uniforme 1 0.
        }
    }

    Post_processing
    {
        Probes
        {
            sonde_pression	pression periode 0.01 points   1   0.45 0.45
            sonde_vitesse	vitesse  periode 0.01 segment 10   0.05 0.5	0.95 0.5
            coupe_vitesse	nodes vitesse  periode 0.01 segment 10   0.9 0.	0.9 1
        }
        fields dt_post 10.
        {
            pression elem
            pression som
            vitesse som
        }
    }
}
Solve pb
End
