# Premier calcul en VDF, creation d'un fichier MED #
# PARALLEL ONLY 2 #
dimension 2
Pb_Thermohydraulique pb
Domaine dom
# BEGIN MESH #
Mailler dom
{
    Pave Cavite
    {
        Origine 0. 0.
        Nombre_de_Noeuds 11 11
        Longueurs 0.01 0.01
    }
    {
        Bord Gauche X = 0.  0. <= Y <= 0.01
        Bord Haut Y = 0.01  0. <= X <= 0.01
        Bord Bas Y = 0.     0. <= X <= 0.01
        Bord Droit X = 0.01 0. <= Y <= 0.01
    }
}
# END MESH #
# BEGIN PARTITION
Partition dom
{
    Partition_tool Metis { nb_parts 3 }
    Larg_joint 2
    zones_name dom0
    ecrire_decoupage dom0.dec
    single_hdf
}
End
END PARTITION #

# BEGIN SCATTER
Scatter dom0.Zones dom
END SCATTER #

VDF dis

Scheme_euler_explicit sch
Read sch
{
    tinit 0.
    tmax 1.2
    dt_min 0.002
    dt_max 0.002
    dt_impr 0.01
    dt_sauv 0.1
    seuil_statio 1.e-8
}

Associate pb dom
Associate pb sch
Discretize pb dis

Read pb
{

    fluide_incompressible {
        gravite champ_uniforme 2 0 -9.81

        mu Champ_Uniforme 1 1.85e-5
        rho Champ_Uniforme 1 1.17
        lambda Champ_Uniforme 1 0.0262
        Cp Champ_Uniforme 1 1006
        beta_th Champ_Uniforme 1 3.41e-3
    }


    Navier_Stokes_standard
    {
        solveur_pression petsc cholesky { }
        convection { quick }
        diffusion { }
        sources { boussinesq_temperature { T0 5. } }
        initial_conditions {
            vitesse Champ_Uniforme 2 0. 0.
        }
        boundary_conditions {
            Haut paroi_fixe
            Bas paroi_fixe
            Gauche paroi_fixe
            Droit paroi_fixe
        }
    }
    Convection_Diffusion_Temperature
    {
        diffusion { }
        convection { quick }
        boundary_conditions
        {
            Haut paroi_adiabatique
            Bas  paroi_adiabatique
            Gauche paroi_temperature_imposee Champ_Front_Uniforme 1 10.
            Droit  paroi_temperature_imposee Champ_Front_Uniforme 1 0.
        }
        initial_conditions { Temperature Champ_Uniforme 1 5. }
    }

    Post_processings
    {
        med {
            fichier initial format med fields dt_post 0.1
            {
                vitesse som
                temperature elem
            }
        }
        lata {
            fichier prepare format lata fields dt_post 0.1
            {
                vitesse som
                temperature som
            }
        }
    }
}
Solve pb
# Conversion du fichier LATA en un fichier MED unique #
lata_to_med NOM_DU_CAS NOM_DU_CAS
End
