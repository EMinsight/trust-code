# Conduction 2D #
# Terme source volumique lineaire #
# Temperature imposee en entree et en sortie #
# Symetrie sur les parois laterales #

# PARALLEL OK 8 #
dimension 2

Pb_conduction pb
Domaine dom
# DEBUT MAILLAGE #
lire_fichier dom dom2_mod.geom
ecrire_med dom dom.med
domaine dom2
lire_med dom2 dom dom.med
# FIN MAILLAGE #
# DEBUT DECOUPAGE
Decouper dom
{
	Partitionneur tranche { tranches 2 1 }
	Larg_joint 1
	Nom_Zones DOM
}
Fin
FIN DECOUPAGE #

# DEBUT LECTURE
Scatter DOM.Zones dom
FIN LECTURE #

EF dis

Schema_Euler_implicite sch
Lire sch
{
	tinit 0
	nb_pas_dt_max 3
	dt_min 0.00001
	dt_sauv 100
	seuil_statio 1.e-8
solveur piso { seuil_convergence_solveur 1e-12 }
facsec 1e4
}

Solide sol
Lire sol
{
	rho Champ_Uniforme 1 1.
	lambda Champ_Uniforme 1 0.25
	Cp Champ_Uniforme 1 1.
}
Associer pb dom
Associer pb sch
Associer pb sol

Discretiser pb dis

Lire pb
{
	Conduction
	{
		diffusion { }
		sources { 
			Puissance_thermique Champ_Fonc_XYZ dom 1 3.*x/2.
   			}
		conditions_initiales {
			temperature Champ_Uniforme 1 0. 
			}
		conditions_limites {
			Droit paroi_temperature_imposee
			      Champ_Front_Uniforme 1 0.
			Gauche paroi_temperature_imposee
			      Champ_Front_Uniforme 1 0.
		}
	}

	Postraitement
	{
/*
		Sondes
		{
			temperature_x temperature periode 5. tous_les_elems
		}
*/
		format lml
		Champs formatte dt_post 30.
		{
			temperature som
		}
	}
}

Resoudre pb

Fin
