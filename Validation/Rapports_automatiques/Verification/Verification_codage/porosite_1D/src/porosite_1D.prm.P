Parameters
{
    Title "Ecoulement avec saut de porosite en 1D"
    Description "Ecoulement unidirectionel, sans diffusion. Le domaine est 1D. On compare VDF, PolyMAC, CoviMAC Ã  la solution qui satisfait la conservation de la charge."
    Author "Y.G."
#Pforeach _dis_ ( VDF PolyMAC CoviMAC )
    TestCase _dis_ "jdd.data"
#Pendforeach(_dis_)
}

Chapter
{
    Title "Residuals"
    Description ""

    Figure
    {
        title_figure residus
        include_description_curves 0
        labelX "t (s)"
        labelY "residuals"
        logY

        width 10cm
        grid
        legend below title "Legende"
#Pforeach _dis_ ( VDF PolyMAC CoviMAC )
        Curve
        {
            legend _dis_
            file ./_dis_/jdd.dt_ev
            columns 1 4
            style lines
            Linewidth 2
        }
#Pendforeach(_dis_)
    }

    Figure
    {
        title_figure vitesse
        include_description_curves 0
        labelX "z (m)"
        labelY "v"

        width 7.5cm
        grid
        legend below title "Legende"
        nb_img_without_newline 2
#Pforeach _dis_ ( VDF PolyMAC CoviMAC )
        Curve
        {
            legend _dis_
            segment ./_dis_/jdd V
            columns 1 3
            style lines
            Linewidth 2
        }
#Pendforeach(_dis_)
        Curve
        {
            legend Analytique
            segment ./VDF/jdd V_ex
            columns 1 3
            style points
        }
    }

    Figure
    {
        title_figure pression
        include_description_curves 0
        labelX "z (m)"
        labelY "v"

        width 7.5cm
        grid
        legend below title "Legende"
        nb_img_without_newline 2
#Pforeach _dis_ ( VDF PolyMAC CoviMAC )
        Curve
        {
            legend _dis_
            segment ./_dis_/jdd P
            columns 1 2
            style lines
            Linewidth 2
        }
#Pendforeach(_dis_)
        Curve
        {
            legend Analytique
            segment ./VDF/jdd P_ex
            columns 1 2
            style points
        }
    }
}
