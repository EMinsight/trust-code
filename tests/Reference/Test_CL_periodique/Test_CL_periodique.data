dimension 3
Pb_Hydraulique_Turbulent pb

Domaine dom

# DEBUT MAILLAGE #
Lire_med family_names_from_group_names dom geometry geometry.med
Tetraedriser_homogene_fin dom
Regroupebord dom section { INLET OUTLET }
Regroupebord dom wall { INNER_WALL OUTER_WALL }
Corriger_frontiere_periodique { domaine dom bord section }
# Fin MAILLAGE #

# DEBUT DECOUPAGE
Decouper dom
{
    Periodique 1 section
    Partitionneur Metis { nb_parts 6 }
    larg_joint 2
    nom_zones DOM
}
FIN MAILLAGE #

# DEBUT LECTURE
Scatter DOM.Zones dom
FIN LECTURE #

# SPATIAL Discretization #
VEFPreP1B dis
Lire dis { P0 P1 Changement_de_base_P1Bulle 1 Modif_div_face_dirichlet 1 }

# TIME DISCRETIZATION #
Schema_Euler_implicite sch
Lire sch {
    tinit   0.
    tmax    10.
    nb_pas_dt_max 1
    dt_min 1.e-6
    dt_max 0.01
    dt_impr 0.01
    dt_sauv 1000.
    seuil_statio 1.e-6
    facsec 20.
    facsec_max 20.
    Solveur implicite {
        solveur gmres { diag seuil 1.e-30 nb_it_max 5 impr }
    }
}

# FLUID DEFINITION #
Fluide_Incompressible water
Lire water {
    mu  Champ_Uniforme 1 1.002e-3
    rho Champ_Uniforme 1 998.2
}

# DEFINE THE ASSOCIATION OF PREVIOUSLY DEFINED OBJECTS #
Associer pb dom
Associer pb water
Associer pb sch 
Discretiser pb dis


# READ THE PROBLEM #
Lire pb
{
    Navier_Stokes_Turbulent {
        solveur_pression petsc cholesky {  }
        convection { EF_stab { alpha 0.2 } }
        diffusion { }
        conditions_initiales { vitesse Champ_Uniforme 3 0. 0. 0.298 }
        sources { Canal_perio { bord section } }
        conditions_limites {
            section     periodique
            wall        paroi_fixe
        }
        Modele_turbulence K_epsilon {
            Transport_K_Epsilon {
                Diffusion { }
                Convection { Amont }
                Conditions_limites {
                    section     Frontiere_ouverte_K_Eps_impose Champ_front_Uniforme 2 1.33e-3 .023
                    wall        paroi
                }
                conditions_initiales { K_eps Champ_Uniforme 2 1.33e-3 .023 }
            }
            Correction_visco_turb_pour_controle_pas_de_temps
            Turbulence_paroi Loi_standard_hydr dt_impr_ustar 0.5
        }
    }

    Postraitement {
        format lata
        champs dt_post 1.e-3 { vitesse faces }
    }
}
Resoudre pb
Fin
