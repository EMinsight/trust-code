# PARALLEL OK #
system "rm -rf lata; mkdir -p lata"
dimension 3
Domaine dom

# BEGIN MESH #
mailler  dom
{
    pave carre
    {
        origine  0.0  0.0 0.0
        nombre_de_noeuds  2 1001 2
        longueurs 1 1 1
    }
    {
        bord  bas       Y = 0.0    0.0 <= X <= 1.0 0. <= Z <= 1.
        bord  haut      Y = 1  0.0 <= X <= 1.0 0. <= Z <= 1.
        bord  boundary  X = 0.0    0.0 <= Y <= 1  0. <= Z <= 1.
        bord  boundary  X = 1.0    0.0 <= Y <= 1  0. <= Z <= 1.
        bord  boundary  Z = 0.0   0. <= X <= 1.  0.0 <= Y <= 1
        bord  boundary  Z = 1.0    0. <= X <= 1. 0.0 <= Y <= 1
    }
}
polyedriser dom
# END MESH #

# BEGIN PARTITION
Partition dom
{
        /* Choose Nb_parts so to have ~ 25000 cells per processor */
        Partition_tool metis { nb_parts 4 }
        Larg_joint 2
        zones_name DOM
}
End
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #

CoviMAC  dis
option_covimac {  }

Schema_euler_implicite sch
Read sch
{
    tinit 0
    tmax 3
    dt_max 1e-4
    seuil_statio -1
    nb_pas_dt_max 100
    dt_impr 1e-10
    max_iter_implicite 1
    solveur ice
    {
        criteres_convergence { alpha 1e-5 pression 1e8 vitesse 1e8 temperature 1e8 }
        iter_min 5
        solveur petsc cli { }
        seuil_convergence_implicite 1e30
    }
}


Pb_Multiphase pb
Associate  pb dom
Associate  pb sch
Discretize pb dis

Read pb
{
    Milieu_composite
    {
        $fluids
    }

    QDM_Multiphase
    {
        solveur_pression petsc cli_quiet { -pc_type hypre -pc_hypre_type boomeramg }
        convection { amont }
        diffusion  { negligeable }
        initial_conditions
        {
            vitesse  Champ_Fonc_xyz dom 3 0 0 1
            pression Champ_Fonc_xyz dom 1 15.5e6
        }
        boundary_conditions
        {
            boundary symetrie
            bas  frontiere_ouverte_vitesse_imposee_sortie champ_front_fonc_t 3 0 0 1
            haut frontiere_ouverte_pression_imposee champ_front_uniforme 1 15.5e6
        }
    }
    Masse_Multiphase
    {
        initial_conditions { alpha Champ_Fonc_xyz dom 1 1 }
        convection { amont }
        boundary_conditions
        {
            boundary paroi
            bas  frontiere_ouverte a_ext Champ_Front_Uniforme 1 1
            haut frontiere_ouverte a_ext Champ_Front_Uniforme 1 1
        }
    }
    Energie_Multiphase
    {
        diffusion { negligeable }
        convection { amont }
        initial_conditions { temperature Champ_Uniforme 1 343 }
        boundary_conditions
        {
            boundary paroi_adiabatique
            bas    frontiere_ouverte T_ext Champ_Front_Uniforme 1 343
            haut   frontiere_ouverte T_ext Champ_Front_Uniforme 1 343
        }
        sources
        {
            travail_pression ,
            puissance_thermique champ_uniforme 1 2000.0
        }
    }
    Post_processing
    {
        Format Lata
        fichier lata/jdd
        fields dt_post 1e-2
        {
            pression elem
        }
    }
}
Solve pb
End
