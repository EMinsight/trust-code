Parameters {
	Title "Echange_impose VEF 2D calculation"
	Author "Pierre LEDAC"
	VersionTrio_U 1.7.7
 	TestCase . Echange_impose_VEF_explicite.data
	TestCase . Echange_impose_VEF_implicite.data
	Description "Boundary exchange heat flux in VEF: HeatFlux=h(Tp-Text)"
	Description "Check the obtained wall temperature: it should be 5 degrees."
	Description "Use explicit and implicit schemes."
}

Chapter {
	Title "Convergence" 

	figure { 
		title "Evolution of residue"
		include_description_curves 0
		labelX "TIME"
		labelY "residu=max|Ri|"
		logX
		logY
		curve {
			file Echange_impose_VEF_explicite.dt_ev
			columns ($1) ($4)
			style linespoints
		}
		curve {
			file Echange_impose_VEF_implicite.dt_ev
			columns ($1) ($4)
			style linespoints
		}
	}
}

Chapter {
	Title "Visualization via Gnuplot" 

	figure { 
		title "SONDE_T"
		include_description_curves 0
		labelX "TIME"
		labelY "TEMPERATURE"
		curve {
			file Echange_impose_VEF_explicite_SONDE_T.son
			columns ($1) ($2)
			style linespoints
		}
		curve {
			file Echange_impose_VEF_implicite_SONDE_T.son
			columns ($1) ($2)
			style linespoints
		}
	}
}

