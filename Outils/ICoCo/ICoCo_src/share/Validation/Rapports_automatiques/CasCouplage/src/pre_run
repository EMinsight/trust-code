#gunzip -f interass.geom.gz
#./configure

make ld_env.sh
# modification pour que ctest -jN passe : on ne regenere pas l'executable, on suppose qu'il a deja ete construit
#exec=`make exe | grep Executable |awk '{print $NF}'`
exec=$exec_opt && [ "$OPT" != "_opt" ] && exec=$exec_debug
export exec

if [ "$1" != "grossier" ]
then
trust dec_canal
trust dec_boite
else
sed "s/10 9 9/4 3 3/" dec_canal.data > dec_c.data
sed "s/10 10 11/5 3 3/" dec_boite.data > dec_b.data
trust dec_c
trust dec_b
fi
gunzip -c ref_lml.gz > ref_lml



make check
status=$?
if [ $status -eq 0 ]
then
  echo OK > is_ok
else
  #echo KO > is_ok
  rm -f is_ok
fi
