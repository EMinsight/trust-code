# Conduction 2D #
# Terme source volumique sinusoidal #
# Temperature imposee en entree et en sortie #
# Symetrie sur les parois laterales #

# PARALLEL OK 8 #
dimension 2

Pb_conduction pb
Domaine dom
# DEBUT MAILLAGE #
Mailler dom
{
	Pave Cavite 
 	{
		Origine 0. 0.
		Nombre_de_Noeuds 101 21 
		Longueurs 2. 1.
	}
	{
		Bord Gauche X = 0. 0. <= Y <= 1.
		Bord Haut   Y = 1. 0. <= X <= 2.
		Bord Bas    Y = 0. 0. <= X <= 2.
		Bord Droit  X = 2. 0. <= Y <= 1.
	}
} 

transformer dom x y+0.3*y*(1.-y)*sin(2.*3.141592654*x/2.)
trianguler dom

# FIN MAILLAGE #
# DEBUT DECOUPAGE
Decouper dom
{
	Partitionneur tranche { tranches 2 1 }
	Larg_joint 1
	Nom_Zones DOM
}
Fin
FIN DECOUPAGE #

# DEBUT LECTURE
Scatter DOM.Zones dom
FIN LECTURE #

EF dis

Schema_Euler_implicite sch
Lire sch
{
	tinit 0
	dt_min 0.00001
	dt_sauv 100
	seuil_statio 1.e-8
solveur piso { seuil_convergence_solveur 1e-12 }
facsec 1e4
}

Solide sol
Lire sol
{
	rho Champ_Uniforme 1 1.
	lambda Champ_Uniforme 1 0.25
	Cp Champ_Uniforme 1 1.
}
Associer pb dom
Associer pb sch
Associer pb sol

Discretiser pb dis

Lire pb
{
	Conduction
	{
		diffusion { }
		sources { 
			Puissance_thermique Champ_Fonc_XYZ dom 1 (3.*sin(3.141592654*x/2.))
   			}
		conditions_initiales {
			temperature Champ_Uniforme 1 0. 
			}
		conditions_limites {
			Haut paroi_adiabatique
			Bas symetrie
			Droit paroi_temperature_imposee
			      Champ_Front_Uniforme 1 0.
			Gauche paroi_temperature_imposee
			      Champ_Front_Uniforme 1 0.
		}
	}

	Postraitement
	{
		Sondes
		{
			temperature_x temperature periode 5. segment 42  0. 0.5 2. 0.5
		}
		format lata
		Champs dt_post 30.
		{
			temperature som
		}
	}
}

Resoudre pb

Fin
