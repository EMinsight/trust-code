# Thermohydraulique 2D VDF avec modele non newtonien Ostwald #
# PARALLEL OK 8 #
dimension 2
Pb_Thermohydraulique pb
Domaine dom 
# BEGIN MESH #
Mailler dom
{
	Pave Plaques
	{
	Origine 0. 0.
	Nombre_de_Noeuds 21 13
	Longueurs 2.19 0.0076
	Facteurs 1.0 1.05
	}
	{
	Bord Gauche X = 0.     0. <= Y <= 0.0076
	Bord Droit  X = 2.19    0. <= Y <= 0.0076
	Bord Bas    Y = 0.        0. <= X <= 2.19  
	Bord Haut   Y = 0.0076  0. <= X <= 2.19  
	}	
}

# END MESH #
# BEGIN PARTITION
Partition dom
{
	Partitionneur tranche { tranches 2 1 }
	Larg_joint 1
	Nom_Zones DOM
}
Fin
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #

VDF dis
schema_Euler_explicite sch
Lire sch
{
	tinit 0.
	tmax 0.02
	dt_min 1.e-7
	dt_max 0.1
	dt_impr  0.001
	dt_sauv 100.
	seuil_statio 1.e-8
}

Fluide_Ostwald fluide
Lire fluide
{
	K Champ_Fonc_Tabule 1 temperature  { 9 20. 25. 30. 35. 40. 45. 50. 55. 60. 4.983711 3.842698  2.962919  2.284563  1.761516  1.358222  1.047257  0.807490  0.622616 }
	rho Champ_Uniforme 1 995
	n Champ_Uniforme 1 0.66
	mu champ_ostwald
	lambda Champ_Uniforme 1 0.623
	Cp Champ_Uniforme 1 4178
	beta_th Champ_Uniforme 1 3.29e-4

}

Associate pb dom
Associate pb sch
Associate pb fluide

Discretize pb dis

Lire pb
{
	Navier_Stokes_Standard
	{
		solveur_pression petsc cholesky { }
		convection { amont }
		diffusion { }	
		conditions_initiales { vitesse Champ_Uniforme 2 1.2 0. }		
		boundary_conditions 
		{
			 Bas Paroi_Fixe
			 Haut Symetrie
			 Droit frontiere_ouverte_pression_imposee Champ_Front_Uniforme 1 0.
			 Gauche frontiere_ouverte_vitesse_imposee Champ_Front_Uniforme 2 0.6875 0. 
		}
	}
	Convection_diffusion_temperature
	{
		convection { Amont }
		diffusion { }		
		boundary_conditions
		{
		Bas paroi_temperature_imposee Champ_Front_Uniforme 1 60.
		Haut symetrie
		Droit Frontiere_ouverte_temperature_imposee Champ_Front_Uniforme 1 20.
		Gauche Frontiere_ouverte_temperature_imposee Champ_Front_Uniforme 1 20.
		}		
		conditions_initiales { temperature Champ_Uniforme 1 20. }
	}
		
	Postraitement 
	{
		Sondes 
		{
			coupe_vitesse1  Vitesse  periode 0.001 segment 26 1. 0.  1. 0.0076
			coupe_vitesse2  Vitesse  periode 0.001 segment 26 2. 0.  2. 0.0076
			sonde_temperature  Temperature  periode 0.001 segment 26 1. 0.  1. 0.0076
		}
		Champs Dt_post 0.3
		{			
			vitesse 
			temperature
			consistance
		}
	}
}

Solve pb
Fin
