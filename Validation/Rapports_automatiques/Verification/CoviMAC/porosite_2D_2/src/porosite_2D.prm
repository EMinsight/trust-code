Parameters
{
    Title "Ecoulement avec saut de porosite"
    Description "Ecoulement unidirectionel, sans diffusion. Le domaine est discretise en 2D. Le calcul est mene sur differents maillages (cartesien conforme, triangles et non conforme)."
    Author "Y.G."
    TestCase cartesian     "jdd.data"
    TestCase triangle      "jdd.data"
    TestCase non_conformal "jdd.data"
}

Chapter
{
    Title "Champs de vitesse"
    Description ""

    visu
    {
        normal3D 0. 0. 1.
        up3D 0.161316 0.944928 -0.28476
        Title "Maillage cartesien"
        mesh        cartesian/jdd.lata dom
        pseudocolor cartesian/jdd.lata dom VITESSE_Y ELEM
        Description ""
    }

    visu
    {
        normal3D 0. 0. 1.
        up3D 0.161316 0.944928 -0.28476
        Title "Maillage de triangles"
        mesh        triangle/jdd.lata dom
        pseudocolor triangle/jdd.lata dom VITESSE_Y ELEM
        Description ""
    }

    visu
    {
        normal3D 0. 0. 1.
        up3D 0.161316 0.944928 -0.28476
        Title "Maillage non conforme"
        mesh        non_conformal/jdd.lata dom
        pseudocolor non_conformal/jdd.lata dom VITESSE_Y ELEM
        Description ""
    }

    Tableau
    {
        Titre "Comparaison theorie/numerique"
        nb_colonnes 3
        label \latex_( $\Vert P \Vert_{\infty}$ \latex_) |  \latex_( $\Vert \vec{v} \Vert_{\infty}$ \latex_) | \latex_( $t_{end}$ \latex_)
        Ligne
        {
            Legende "Cartesien"
            nb_colonnes_fichier 3
            fichier ./cartesian/erreurs.txt
            colonnes $2 $3 $1
        }
        Ligne
        {
            Legende "Triangles"
            nb_colonnes_fichier 3
            fichier ./triangle/erreurs.txt
            colonnes $2 $3 $1
        }
        Ligne
        {
            Legende "Non conforme"
            nb_colonnes_fichier 3
            fichier ./non_conformal/erreurs.txt
            colonnes $2 $3 $1
        }
    }
}
