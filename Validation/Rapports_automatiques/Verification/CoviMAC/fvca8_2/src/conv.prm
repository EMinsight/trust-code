Parameters
{
    Title "Benchmark FVCA8"
 TestCase Triangles/jdd_1 cas.data
 TestCase Quadrangles/jdd_1 cas.data
 TestCase Cartesian/jdd_1 cas.data
 TestCase Cartesian/jdd_2 cas.data
 TestCase Locally_Refined/jdd_1 cas.data
 TestCase Locally_Refined/jdd_2 cas.data
 TestCase Voronoi/jdd_1 cas.data
 TestCase Prism/jdd_1 cas.data
 TestCase Hexa/jdd_1 cas.data
 TestCase Tetra/jdd_1 cas.data
 TestCase PrismHexa/jdd_1 cas.data
 TestCase Kershaw/jdd_1 cas.data
 TestCase LocRaf/jdd_1 cas.data
 TestCase Random/jdd_1 cas.data
 TestCase CheckerBoard/jdd_1 cas.data
# bloc testcases #

    Author "A. Gerschenfeld, Y. Gorsse"
    IncludeData 2
    Prerequisite "./extract_convergence"
    Description "Cas tests de Stokes 2D sur les maillages FVCA8, et Navier-Stokes 3D sur maillages FVCA8-3D et FVCA6-3D"
}

Chapter
{
    Title "Stokes stationnaire 2D : Bercovier-Engelman."


        visu
    {
        normal3D 0. 0. 1.
        mesh Triangles/jdd_1/cas.lata dom
        Description "Triangles"
        nb_img_without_newline 2
        width 7cm
    }
        visu
    {
        normal3D 0. 0. 1.
        mesh Quadrangles/jdd_1/cas.lata dom
        Description "Quadrangles"
        nb_img_without_newline 2
        width 7cm
    }
        visu
    {
        normal3D 0. 0. 1.
        mesh Cartesian/jdd_1/cas.lata dom
        Description "Cartesian"
        nb_img_without_newline 2
        width 7cm
    }
        visu
    {
        normal3D 0. 0. 1.
        mesh Locally_Refined/jdd_1/cas.lata dom
        Description "Locally_Refined"
        nb_img_without_newline 2
        width 7cm
    }
        Figure
    {
        labelx N
        labely erreur max
        logx
        logy
        Include_Description_Curves 0
        width 12cm
        Title "Ordres de convergence vitesse"
                Curve
        {
            legend Triangles
            file Triangles/conv_glob_v
            colonnes (1./sqrt($1)) $2
            style linespoints
        }
                Curve
        {
            legend Quadrangles
            file Quadrangles/conv_glob_v
            colonnes (1./sqrt($1)) $2
            style linespoints
        }
                Curve
        {
            legend Cartesian
            file Cartesian/conv_glob_v
            colonnes (1./sqrt($1)) $2
            style linespoints
        }
                Curve
        {
            legend Locally_Refined
            file Locally_Refined/conv_glob_v
            colonnes (1./sqrt($1)) $2
            style linespoints
        }
                Curve
        {
            legend Order 1
            fonction x
            style lines
        }
        Curve
        {
            legend Order 2
            fonction x*x
            style lines
        }
    }

    Figure
    {
        labelx N
        labely erreur max
        logx
        logy
        Include_Description_Curves 0
        width 12cm
        Title "Ordres de convergence pression"
                Curve
        {
            legend Triangles
            file Triangles/conv_glob_p
            colonnes (1./sqrt($1)) $2
            style linespoints
        }
                Curve
        {
            legend Quadrangles
            file Quadrangles/conv_glob_p
            colonnes (1./sqrt($1)) $2
            style linespoints
        }
                Curve
        {
            legend Cartesian
            file Cartesian/conv_glob_p
            colonnes (1./sqrt($1)) $2
            style linespoints
        }
                Curve
        {
            legend Locally_Refined
            file Locally_Refined/conv_glob_p
            colonnes (1./sqrt($1)) $2
            style linespoints
        }
                Curve
        {
            legend Order 1
            fonction x
            style lines
        }
        Curve
        {
            legend Order 2
            fonction x*x
            style lines
        }
    }
}

