#include <NOM_DU_FICHIER.h>
#include <TYPE_ZONE_DIS.h>

implemente_instanciable(NOM_DE_LA_CLASSE,
                        "NOM_DE_LA_CLASSE",
                        Champ_Inc_base)

// printOn et readOn 

Sortie& NOM_DE_LA_CLASSE::printOn(Sortie& s ) const 
{
   return s << que_suis_je() << "\n";
}

Entree& NOM_DE_LA_CLASSE::readOn(Entree& is ) 
{
  lire_donnees(is) ;
  return is ;
}


const Zone_dis_base& NOM_DE_LA_CLASSE::zone_dis_base() const
{
  return LA_ZONE_DIS.valeur();
}

const Zone_dis_base& NOM_DE_LA_CLASSE::associer_zone_dis_base(const Zone_dis_base& zone_dis_base)
{ 
   LA_ZONE_DIS = ref_cast(TYPE_ZONE_DIS,zone_dis_base) ;
   return zone_dis_base;
}
   
DoubleTab& NOM_DE_LA_CLASSE::remplir_coord_noeuds(DoubleTab& x) const
{
  // A CODER  
  return x;
}

