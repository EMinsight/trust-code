#!/bin/bash
# Recupere le fichier provenant de Mosaic :
MAILLAGE=`head -1 $1 | $TRUST_Awk '{print $2}'`
follow="-follow" && [ $TRUST_ARCH = RS6000 ] && follow=""
MAILLAGE=`find $GIBI_ROOT $follow -name $MAILLAGE -print`
if [ ! -f $MAILLAGE ]
then
   Erreur.ihm "Fichier $MAILLAGE inexistant ou pas d'en tete dans le fichier $MAILLAGE."
fi
ETUDE_TRUST=`QUELLE_ETUDE.ihm`
export ETUDE_TRUST
$Xterm -title "Copier" -e copie_fichier.ihm $MAILLAGE 2>/dev/null
