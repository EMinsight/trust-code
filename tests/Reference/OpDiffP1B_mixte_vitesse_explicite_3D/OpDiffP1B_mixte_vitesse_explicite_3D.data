# Hydraulique 3D VEFPreP1B #
# PARALLEL RUNS #

dimension 3
Domaine dom_pbF1

# BEGIN MESH #
Mailler dom_pbF1
{
       Pave domF
               {
               Origine 0. 0. 0.
               Nombre_de_Noeuds 3 3 3
               Longueurs 1. 1. 1.
               }
               {
               bord perio       X = 0  0 <= Y <= 1   0 <= Z <= 1
	       bord perio       X = 1  0 <= Y <= 1   0 <= Z <= 1 
               bord Devant      Y = 0  0 <= X <= 1   0 <= Z <= 1
               bord Derriere    Y = 1  0 <= X <= 1   0 <= Z <= 1
	       bord Bas         Z = 0  0 <= X <= 1   0 <= Y <= 1
	       bord Haut        Z = 1  0 <= X <= 1   0 <= Y <= 1
               }
}
tetraedriser_homogene dom_pbF1
Reordonner_Faces_Periodiques dom_pbF1 perio
# END MESH #

Pb_Hydraulique pbF1

# BEGIN PARTITION
Partition dom_pbF1
{
        Partitionneur tranche { tranches 4 1 1 }
        Larg_joint 2
        Nom_Zones DOM
	periodique 1 perio
}
End
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom_pbF1
END SCATTER #


Scheme_euler_explicit schG
Read schG
{
       tinit 0.
       tmax 1000.
       dt_min 1.e-6
       dt_max 1.
       dt_impr 0.1
       dt_sauv 100000
       seuil_statio 1.e-8
       facsec 1.
       nb_pas_dt_max 10
}

VEFPreP1b cubesregu

Fluide_Incompressible eau
Read eau
{
       rho Champ_Uniforme 1 1.
       mu Champ_Uniforme 1 1.
}


Associate pbF1 dom_pbF1
Associate pbF1 eau  
Associate pbF1 schG

Discretize pbF1 cubesregu

Read pbF1
{
Navier_Stokes_standard
       {
               solveur_pression GCP { precond ssor { omega 1.5 } seuil 1.e-12 }
       	       convection { negligeable }
               diffusion { P1NCP1B {  } }
               conditions_initiales {
                       vitesse Champ_fonc_txyz dom_pbF1 3 y z 0.
                       }
               boundary_conditions {
                   perio periodique 
                   Haut  Symetrie 
                   Bas   Symetrie
		   Devant   frontiere_ouverte_vitesse_imposee Champ_Front_fonc_txyz 3 0 z 0.
		   Derriere frontiere_ouverte_vitesse_imposee Champ_Front_fonc_txyz 3 1. z 0.
               }


       }
       Postraitements
       {
       		lml
		{
       			format lml
       			fichier OpDiffP1B_mixte_vitesse_explicite_3D
       			Champs dt_post 1.
               		{
                 	vitesse elem
               		}
       		}
		lata
		{
		       	format lata
       			Champs dt_post 1.
               		{
                 	vitesse faces
               		}
		}	
       } 
       sauvegarde formatte pbF.sauv             
}

Solve pbF1
End


