# Thermohydraulique 2D couplee a conduction 2D #
# PARALLEL OK 8 #

dimension 2

Domaine dom1
Domaine dom2

# BEGIN MESH #
Mailler dom1 {
    Pave Cavite1 {
        Origine 0 0
        Nombre_de_Noeuds 5 5
        Longueurs 1 1
    }
    {
        Bord Gauche1 X = 0  0 <= Y <= 1
        Bord Sortie   Y = 1  0 <= X <= 1
        Bord Entree    Y = 0  0 <= X <= 1
        raccord local homogene Droit1  X = 1  0 <= Y <= 1
    }
}

Mailler dom2 {
    Pave Cavite2 {
        Origine 1 0
        Nombre_de_Noeuds 5 5
        Longueurs 1 1
    }
    {
        raccord local homogene Droit1   X = 1  0 <= Y <= 1
        Bord Haut2     Y = 1  1 <= X <= 2
        Bord Bas2      Y = 0  1 <= X <= 2
        Bord Droit2    X = 2  0 <= Y <= 1
    }
}

# END MESH #

# BEGIN PARTITION
Partition dom1
{
    Partition_tool metis { Nb_parts 2 }
    Larg_joint 1
    zones_name DOM1
}
Partition dom2
{
    Partition_tool metis { Nb_parts 2 }
    Larg_joint 1
    zones_name DOM2
}
End
END PARTITION #

# BEGIN SCATTER
Scatter DOM1.Zones dom1
Scatter DOM2.Zones dom2
END SCATTER #

VDF dis

Pb_thermohydraulique pb1
Pb_conduction pb2

Sch_CN_iteratif sch
Read sch {
    tinit 0
    nb_pas_dt_max 10
    dt_start dt_calc
    facsec_max 1.9
    dt_impr 0.1
    dt_sauv 1000
}

Associate pb1 dom1
Associate pb2 dom2

Probleme_Couple pbc
Associate pbc pb1
Associate pbc pb2
Associate pbc sch
Discretize pbc dis

Read pb1 {
    Fluide_incompressible
    {
        mu Champ_Uniforme 1 0.001
        rho Champ_Uniforme 1 10
        lambda Champ_Uniforme 1 1
        Cp Champ_Uniforme 1 1000
        beta_th Champ_Uniforme 1 0
    }


    Navier_Stokes_standard {
        solveur_pression GCP {
            precond ssor { omega 1.5 }
            seuil 1e-14
        }
        convection { amont }
        diffusion { }
        initial_conditions { vitesse Champ_Uniforme 2 0 0 }
        boundary_conditions {
            Entree frontiere_ouverte_vitesse_imposee Champ_front_Uniforme 2 0 0.1
            Sortie frontiere_ouverte_pression_imposee Champ_front_Uniforme 1 0
            Droit1 paroi_fixe
            Gauche1 paroi_fixe
        }
    }

    Convection_Diffusion_Temperature {
        diffusion { }
        convection { amont }
        initial_conditions { temperature Champ_Uniforme 1 0 }
        boundary_conditions {
            Entree frontiere_ouverte_temperature_imposee Champ_front_Uniforme 1 0
            Sortie frontiere_ouverte T_ext champ_front_uniforme 1 0
            Droit1 Paroi_contact pb2 Droit1
            Gauche1 paroi_adiabatique
        }
    }

    Post_processing {
        fields dt_post 10 {
            temperature elem
            vitesse elem
            pression elem
        }
    }

}

Read pb2 {

    Solide
    {
        rho Champ_Uniforme 1 20
        lambda Champ_Uniforme 1 1
        Cp Champ_Uniforme 1 1000
    }


    Conduction {
        diffusion { }
        initial_conditions {
            temperature Champ_Uniforme 1 30
        }
        boundary_conditions {
            Droit1 Paroi_contact pb1 Droit1
            Haut2 paroi_adiabatique
            Bas2 paroi_adiabatique
            Droit2 paroi_adiabatique
        }
    }
    Post_processing {
        fields dt_post 10 {
            temperature elem
        }
    }
}

Solve pbc
End
