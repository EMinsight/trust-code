# Thermohydraulique 2D VEF P1NC/P1Bulle Runge_Kutta_3+centre4 Boussinesq #
# l'ecart en parallel sur la pression est un simple decalage d'une constant #
# PARALLEL OK 8 #
dimension 2
Pb_Thermohydraulique pb

Domaine dom
# BEGIN MESH #
Read_file dom GAMM1.geom
# END MESH #
# BEGIN PARTITION
Partition dom
{
	Partitionneur metis { Nb_parts 2 }
	Larg_joint 2
	Nom_Zones DOM
}
Fin
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #

VEFPreP1B dis

# Schema_MacCormack sch #
Runge_Kutta_ordre_3 sch
Lire sch
{
	nb_pas_dt_max 10
	tinit 0.
	tmax  0.1
	dt_min 0.0001
	dt_max 0.1
	dt_impr 0.001
	dt_sauv 50
	seuil_statio 1.e-8
        facsec 1.0
}

Fluide_Incompressible fluide
Lire fluide
{
	mu Champ_Uniforme 1 0.001094
	rho Champ_Uniforme 1 2
	lambda Champ_Uniforme 1 2.64e-3
	Cp Champ_Uniforme 1 500
	beta_th Champ_Uniforme 1 3.41e-3
}

Champ_Uniforme gravite
Lire gravite 2 0 -9.81

Associate fluide gravite
Associate pb dom
Associate pb sch
Associate pb fluide

Discretize pb dis

Lire pb
{
	Navier_Stokes_standard
	{
		solveur_pression GCP { 
			precond ssor { omega 1.500000 } 
			seuil 1.000000e-15 
			impr 
		}
		convection { centre4 }
		diffusion { }
		sources { boussinesq_temperature { T0 0.5 } }
		conditions_initiales {
			vitesse Champ_Uniforme 2 0. 0.
			}
		boundary_conditions {
			HAUT   paroi_fixe
			BAS    paroi_fixe
			ENTREE paroi_fixe
			SORTIE paroi_fixe
		}
	}
	Convection_Diffusion_Temperature
	{
		diffusion { negligeable }
		convection { negligeable }
		boundary_conditions 
		{
			HAUT   Paroi_adiabatique
			BAS    Paroi_adiabatique
 			ENTREE Paroi_temperature_imposee Champ_Front_Uniforme 1 1.00
			SORTIE Paroi_temperature_imposee Champ_Front_Uniforme 1 0.00
		}
		conditions_initiales { Temperature champ_fonc_xyz dom 1 1-x/4 }
	}

	Postraitement
	{
		Sondes
		{
			sonde_pression pression periode 0.00001 points 1 2.00 0.5
			sonde_vitesse vitesse periode 0.00001 points 5 0.75 0.5
                                                                    2.00 0.19
                                                                    2.00 0.5
                                                                    2.00 0.81
                                                                    3.25 0.5
			sonde_temp temperature periode 0.0001 segment 60 0. 0.501 4. 0.501
                        sonde_vite vitesse     periode 0.0001 segment 60 0. 0.501 4. 0.501
			
			sonde_vit_min vitesse_reduction_min periode 0.00001 points 1 2.00 0.5
			sonde_vit_max vitesse_reduction_max periode 0.00001 points 1 2.00 0.5
			sonde_vit_moy vitesse_reduction_moy periode 0.00001 points 1 2.00 0.5
		}
		
		definition_champs {
		
		  temperature_reduction_min Interpolation { localisation elem
		    source Reduction_0D {
		  	 methode min
			 source refChamp { Pb_champ pb temperature }
		  	}
		  }	
		  temperature_reduction_max Interpolation { localisation elem
		     source Reduction_0D {
		  	 methode max
			 source refChamp { Pb_champ pb temperature }
		  	}
		   }	
		  temperature_reduction_moy Interpolation { localisation som
		     source Reduction_0D {
		  	 methode moyenne
			 source refChamp { Pb_champ pb temperature }
		  	}	
		   }
		   vitesse_reduction_min Interpolation { localisation elem
		    source Reduction_0D {
		  	 methode min
			 source refChamp { Pb_champ pb vitesse }
		  	}
		   }	
		   vitesse_reduction_max Interpolation { localisation elem
		    source Reduction_0D {
		  	 methode max
			 source refChamp { Pb_champ pb vitesse }
		  	}
		   }
		   vitesse_reduction_moy Interpolation { localisation elem
		    source Reduction_0D {
		  	 methode moyenne
			 source refChamp { Pb_champ pb vitesse }
		  	}
		   }
		  
		}
		
		Champs dt_post 50.0
		{
			pression 
			vitesse 
			temperature 
			temperature_reduction_min
			temperature_reduction_max
			temperature_reduction_moy
		}
	}
}

Solve pb
Fin
Fin
