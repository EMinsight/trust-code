# Hydraulique 3D Force de pression: D=1 m U=1 m/s Re=50 #
# PARALLEL OK #

dimension 3

Pb_Hydraulique pb
Domaine dom

# BEGIN MESH #
Read_file dom cylindre.geom
VerifierCoin dom { }
Dilate dom 1000
# END MESH #

# BEGIN PARTITION
Partition dom
{
	Partitionneur metis { Nb_parts 2 }
	Larg_joint 2
	Nom_Zones DOM
}
Fin
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #

VEFPreP1B ma_discretisation

Schema_Euler_implicite mon_schema
Lire mon_schema
{
	nb_pas_dt_max 4
	tinit 0
	tmax 100
	dt_min 1.e-6
	dt_max 100.
	dt_impr 1.e-6
	dt_sauv 100
	seuil_statio 1.e-3
	facsec_max 10
	max_iter_implicite 50
	
	Solveur 
           Simple
	   {
		seuil_convergence_solveur 1.e-8
		relax_pression 0.01
	   }
}

Fluide_Incompressible milieu
Lire milieu
{
	mu  Champ_Uniforme 1 0.04
	rho Champ_Uniforme 1 2.
}

Associate pb dom
Associate pb mon_schema
Associate pb milieu
Discretize pb ma_discretisation

Lire pb
{
	Navier_Stokes_standard
	{
		solveur_pression GCP { 
			precond ssor { omega 1.500000 } 
			seuil 1.000000e-12
			
		}
		convection { muscl }
		diffusion { }
		conditions_initiales {
			vitesse Champ_Uniforme 3 1. 0. 0.
			}
		boundary_conditions {
			Cylindre paroi_fixe
			Surfanz symetrie
			Surfa  symetrie
		      	Bas  symetrie
			Haut symetrie
			Sortie frontiere_ouverte_pression_imposee Champ_front_Uniforme 1 0.
			Entree frontiere_ouverte_vitesse_imposee Champ_front_Uniforme 3 1. 0. 0.
		}

	}

	Postraitement
	{
		Sondes
		{
	       	  sonde_pression pression_pa periode 0.005 circle 11 0. 0. 0. 2 0.7 0. 360.
		  sonde_vitesse vitesse periode 0.005 point 1 0.7 0. 0.
		}
		
		Format lml
		Champs dt_post 1.
		{ 
		  pression_pa elem
		  pression_pa som
		  vitesse elem
		}
			
	}
	
}

Solve pb
Fin
