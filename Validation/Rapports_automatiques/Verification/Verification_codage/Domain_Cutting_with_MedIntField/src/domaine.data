dimension 3

domaine dom

# BEGIN MESH #
Lire_MED {
    domain dom
    file mesh.med
}
# END MESH #

# BEGIN PARTITION #
Postraiter_domaine
{
    fichier mesh.lata
    format lata
    domaine dom
}

Partition dom
{
    Partition_tool Fichier_MED { file Processor.med field Thread }
    Larg_joint 2
    zones_name DOM
}
End
# END PARTITION #

