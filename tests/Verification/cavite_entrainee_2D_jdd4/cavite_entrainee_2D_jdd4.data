# PARALLEL OK #
dimension 2
Pb_hydraulique pb
Domaine dom

# BEGIN MESH #
Lire_MED { 
   domain dom 
   file mesh.med 
}
# END MESH #

# BEGIN PARTITION
Postraiter_domaine
{
    fichier mesh.lata
    format lml
    domaine dom
}

Partition dom
{
    Partition_tool tranche { tranches 2 1 }
    larg_joint 2
    zones_name DOM
}
End
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #

PolyMAC dis

Schema_euler_implicite sch
Read sch
{
    tinit 0
	nb_pas_dt_max 3
    dt_max  0.1
    facsec 1e30
    seuil_statio 1e-10
    solveur implicite { solveur petsc cli { } }
}

Associate  pb dom
Associate  pb sch
Discretize pb dis

Read pb
{
       
fluide_incompressible {
     mu  Champ_Uniforme 1 2.5
    rho Champ_Uniforme 1 1000
}


Navier_Stokes_standard
    {
        methode_calcul_pression_initiale avec_sources_et_operateurs
        solveur_pression petsc cholesky { }
        convection { amont }
        diffusion  { }
        initial_conditions { vitesse Champ_Uniforme 2 0 0 }
        boundary_conditions
        {
            up    paroi_defilante Champ_Front_Uniforme 2 1 0
            down  paroi_fixe
            left  paroi_fixe
            right paroi_fixe
        }
    }

    Post_processing
    {
        probes
        {
            vx grav vitesse periode 0.5 segment 500  0.5 0 0.5 1
            vy grav vitesse periode 0.5 segment 500  0 0.5 1 0.5
        }

        Format lml
        fields dt_post 10
        {
            pression_pa elem
            vitesse elem
        }
    }
    sauvegarde formatte ascci.sauv
}
Solve pb
End
 
