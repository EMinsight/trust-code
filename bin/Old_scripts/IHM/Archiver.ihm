#!/bin/bash
# Archiver une etude
ETUDE_TRUST=`QUELLE_ETUDE.ihm`
export ETUDE_TRUST
if [ ! -f $ETUDE_TRUST/.NOM_DATA ]
then
   Erreur.ihm "Calcul non encore lance !"
else
   cd $ETUDE_TRUST
   # Ameliorer en copiant le fichier dans .ordre_IHM
   echo $ECHO_OPTS "A" > .ordre_IHM
   if [ ! -f .pcs_Run_sonde ]
   then
      Erreur.ihm "Activate the session by clicking on a probe then push button Rename."
   else
      kill -15 `cat .pcs_Run_sonde`
   fi
fi
