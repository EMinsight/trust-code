# PARALLEL OK 2 #
dimension 2
Pb_Hydraulique pb
Domaine dom
# BEGIN MESH #
Mailler dom
{   Pave Cavite
    {
        Origine 0. 0.
        Nombre_de_Noeuds 11 11
        Longueurs 10 2.
    }
    {
        Bord entree  X = 0.            	0. <= Y <= 2.0
        Bord sortie  X = 10       	0. <= Y <= 2.0
        Bord bas     Y = 0.            	0. <= X <= 10
        Bord haut    Y = 2.           	0. <= X <= 10
    }
}

# END MESH #
# BEGIN PARTITION
Partition dom
{
    Partition_tool tranche { tranches 2 1 }
    Larg_joint 2
    zones_name DOM
}
End
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #

VDF dis
Scheme_euler_explicit sch
Read sch
{
    nb_pas_dt_max 3
    tinit 0
    tmax 32
    dt_impr 0.1
    dt_sauv 1000.
    seuil_statio 1.e-8
}
Associate pb dom
Associate pb sch
Discretize pb dis
Read pb
{
       
fluide_incompressible {
     mu Champ_Uniforme 1 3.7e-05
    rho Champ_Uniforme 1 1000
}

Navier_Stokes_standard
    {
        solveur_pression petsc cholesky { }
        convection { quick }
        diffusion { }
        initial_conditions {
            vitesse Champ_Uniforme 2 0. 0.
        }
        boundary_conditions {
            entree frontiere_ouverte_vitesse_imposee champ_front_debit Champ_Front_Uniforme 1  5
            sortie frontiere_ouverte_pression_imposee champ_front_uniforme 1 0
            bas  paroi_fixe
            haut   paroi_fixe

        }
    }
    Post_processing
    {
        format lml
        fields dt_post 1.
        {
            vitesse elem
        }
    }
}

Solve pb
End
 