#!/bin/bash
[ $# != 2 ] && echo Usage $0 domain_name file_err && exit -1
echo "# " generated by : `basename $0` $*
debut=`grep -n "Histogram of the largest angle of each element found into the mesh $1 :" $2 | tail -1 | $TRUST_Awk -F: '{print $1}'`
$TRUST_Awk -v debut=$debut 'BEGIN { fin=0} {if (fin==0) {if (FNR>debut) {if ($1!="Between") { fin=1} else {print $0}  } } }' $2 | sed "s/°//g" 
 
