# Hydraulique 3D VEF P1NCP1P0Pa #
# PARALLEL OK #
dimension 3
Domaine dom

# BEGIN MESH #
Mailler dom
{
Pave Cavite
        {
                Origine 0.  0. 0.
                Nombre_de_Noeuds 5 5 5
                Longueurs 1 1 1
        }
        {
                Bord paroi   X = 0.   0. <= Y <= 1.   0. <= Z <= 1
                Bord paroi  X = 1.  0. <= Y <= 1.   0. <= Z <= 1
                Bord paroi   Y =  0.  0. <= X <= 1.  0. <= Z <= 1
                Bord paroi   Y =  1.  0. <= X <= 1.  0. <= Z <= 1
                Bord paroi  Z = 0.  0. <= X <= 1.   0. <= Y <= 1.
                Bord paroi  Z = 1 0. <= X <= 1.   0. <= Y <= 1.
        }
} 
Tetraedriser_homogene dom
Transformer dom
x+4*x*(1-x)*y*(1-y)*z*(1-z)*(0.5-y)
y+4*x*(1-x)*y*(1-y)*z*(1-z)*(0.5+x)
z

# END MESH #
# BEGIN PARTITION
Partition dom
{
	Partitionneur Metis { Nb_parts 2 }
	Larg_joint 2
	Nom_Zones DOM
}
Fin
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #

VEFPreP1b dis
Lire dis { P0 P1 Pa }

Schema_Euler_Explicite sch
Lire sch 
{
        tinit 0.1
        tmax 1.
        dt_min 1.e-5
        dt_max 1.e-1
	dt_start dt_calc
        dt_impr 0.1
        dt_sauv 20.
        seuil_statio 1.e-8  
        facsec 0.5
}
Fluide_Incompressible fluide
Lire fluide
{
	mu Champ_Uniforme	1 1.
	rho Champ_Uniforme	1 1.
}

Pb_Hydraulique pb
Associate pb dom
Associate pb sch
Associate pb fluide
 
Discretize pb dis
Lire pb
{
	Navier_Stokes_standard
	{
		Solveur_Pression Petsc GCP { precond spai { level 1 epsilon 0.1 } seuil 1.e-10 impr }
                convection { negligeable }
                diffusion  { negligeable }
				
                Sources { Source_Qdm Champ_fonc_xyz dom 3 x 0 0
                        }
		conditions_initiales
			{
                        vitesse Champ_fonc_xyz dom 3 0. 0. 0. 
			}
		boundary_conditions
			{
			paroi paroi_fixe
			}
	}

	Postraitement
	{
                Sondes
                { 
                        sonde_pression pression periode 0.01 segment 10 0. 0.5 0.5 	1. 0.5 0.5
		}
		Champs dt_post 2.
			{
			pression som
			}
	}
}
Solve pb
Fin
