# Hydraulique 2D avec transport 1 constituant et terme source #
# PARALLEL OK 5 #
dimension 2

Pb_hydraulique_concentration pb
Domaine dom

# BEGIN MESH #
Mailler dom
{
	Pave Cavite 
		{
		Origine 0. 0.
		Nombre_de_Noeuds 11 11 
		Longueurs 1. 1.
		}
		{
		Bord Gauche X = 0. 0. <= Y <= 1.
		Bord Haut   Y = 1. 0. <= X <= 1.
		Bord Bas    Y = 0. 0. <= X <= 1.
		Bord Droit  X = 1. 0. <= Y <= 1.
		}
}
trianguler_H dom
# END MESH #
# BEGIN PARTITION
Partition dom
{
	Partition_tool tranche { tranches 2 1 }
	Larg_joint 2
	zones_name DOM
}
End
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #

VEFPreP1B dis

Scheme_euler_explicit sch
Read sch
{
	tinit 0
	tmax 0.2
	dt_min 0.001
	dt_max 0.01
	dt_impr 0.01
	dt_sauv 100
	seuil_statio 1.e-8
}

Fluide_Incompressible fluide
Read fluide
{
	mu Champ_Uniforme 1 3.7e-05
	rho Champ_Uniforme 1 2.
	beta_co Champ_Fonc_xyz dom 1 0.
}

Constituant c
Read c
{
coefficient_diffusion Champ_Uniforme 1 0.01
}

Champ_Uniforme gravite
Read gravite 2 0 0

Associate fluide gravite
Associate pb dom
Associate pb sch
Associate pb fluide
Associate pb c

Discretize pb dis

Read pb
{
	Navier_Stokes_standard
	{
	 	solveur_pression GCP { 
			precond ssor { omega 1.500000 } 
			seuil 1.000000e-06 
			impr 
		}
		convection { muscl }
		diffusion { }
		initial_conditions {
			vitesse Champ_Uniforme 2 1. 1.
		}
		boundary_conditions {
			Haut frontiere_ouverte_vitesse_imposee Champ_Front_Uniforme 2  1. 1.
			Droit frontiere_ouverte_vitesse_imposee Champ_Front_Uniforme 2  1. 1.
			Bas frontiere_ouverte_vitesse_imposee Champ_Front_Uniforme 2  1. 1.
			Gauche frontiere_ouverte_vitesse_imposee Champ_Front_Uniforme 2  1. 1.
		}
		
	}
	Convection_diffusion_Concentration {
	
		Sources { source_Constituant Champ_fonc_xyz dom 1 2*x*y }
		diffusion { negligeable }
		convection { muscl }
		
		initial_conditions {
			concentration Champ_Uniforme 1 0.
		}
		boundary_conditions {
			Haut frontiere_ouverte C_ext Champ_Front_Uniforme 1 0.
			Droit frontiere_ouverte C_ext Champ_Front_Uniforme 1 0.
			Gauche frontiere_ouverte_concentration_imposee Champ_Front_Uniforme 1 0.
			Bas frontiere_ouverte_concentration_imposee Champ_Front_Uniforme 1 0.
		}
	}

	Post_processing
	{
		Probes
		{
			sonde_pres pression periode 0.01 points 1 0.45 0.45
			sonde_vit vitesse periode 0.01 points 1 0.4 0.45
			sonde_conc concentration periode 0.01 points 1 0.55 0.45
			sonde_seg_gradient gradient_test_concentration periode 0.01 segment 10 0. 0.45 1. 0.45
			sonde_moy_grad Moyenne_gradient_conc_som_dom periode 0.01 points 1 0.8 0.6
		}
		
		Definition_champs
		{
		 gradient_test_concentration Gradient {
		 		         source refChamp { Pb_champ pb concentration } 
		 }
		 			 
		 Moyenne_gradient_conc_som_dom Interpolation {
		  			localisation som
		 			source Moyenne {
		 			  sources_reference { gradient_test_concentration  } t_deb 0.1 t_fin 0.2
		 			}			 
		 }
		}
		
		fields dt_post 0.3
		{
			pression elem
			pression som
			vitesse elem
			vitesse som
			Concentration elem
			gradient_test_concentration
			Moyenne_gradient_conc_som_dom
		}
	}
}

Solve pb
End

