# Test du schema EF_stab en 2D #
# PARALLEL OK #
dimension 2
Pb_Hydraulique pb
Domaine dom

# BEGIN MESH #
Read_file dom dom.geom
# END MESH #

# BEGIN PARTITION
Partition dom
{
	Partition_tool Metis { Nb_parts 3 }
	Larg_joint 2
	zones_name DOM
}
End
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #

VEFPreP1b dis

Scheme_euler_explicit sch
Read sch 
{
	nb_pas_dt_max 100
        tinit 0.
        tmax 2.
        dt_min 1.e-7
        dt_max 1.e-1
	dt_start dt_calc
        dt_impr 0.001
        dt_sauv 20.
        seuil_statio 1.e-8
        facsec 0.5
}



Fluide_Incompressible fluide
Read fluide
{
        mu Champ_Uniforme 1  3.465e-05
        rho Champ_Uniforme 1 4.9433
}

Associate pb dom
Associate pb sch
Associate pb fluide

Discretize pb dis
# Debog pb seq faces 1.e-6 0 #
Read pb
	{
	Navier_Stokes_standard
		{
                solveur_pression GCP {
                        precond ssor { omega 1.5 }
                        seuil 1.e-10 impr
                }
                convection { EF_stab { alpha 0.5 } }
                diffusion  { negligeable }
		initial_conditions
			{
                        vitesse Champ_fonc_xyz dom 2  0. 0. 
			}
		boundary_conditions
			{
			cl_13 frontiere_ouverte_vitesse_imposee Champ_front_fonc_xyz  2 -1.5 -2.598  
			cl_12 frontiere_ouverte_pression_imposee Champ_front_uniforme 1 0.
			cl_14 frontiere_ouverte_pression_imposee Champ_front_uniforme 1 0. 
			cl_15 paroi_fixe
			}
	}

	Post_processing
        	{
		format lml       
		fields dt_post 1.e-1
			{
			vitesse elem
			pression elem
			}
		Probes
		{
		 sonde_pression pression periode  1.e-7  segment 60  
			 					    -0.029 -0.027 
								    -0.029  0.027
		}
	}
}

Solve pb
End
