Parameters {
	Title "Test de raffiner_simplex"
	Author "fauchet "
	description On essaye en quadrangle, triangle, hexa et tetra
	TestCase tri 2D
	TestCase quad 2D
	TestCase hexa 3D
	TestCase tetra 3D
	TestCase . quality_3D
	TestCase . raf_iso
	TestCase quad_vef 2D
	TestCase hexa_vef 3D
	TestCase . ver_diag  
	Prerequisite Extract_histogram domain_5 quality_3D.out  > histo
}
