Chapter
{
    Title "Ordres de convergence"
    Description " "

    Figure
    {
        labelx "Ne^{-1/2}"
        labely erreur L2
        logx
        logy
        Include_Description_Curves 0
        legend below title "Legend"
        width 15cm
        Title "Ordres de convergence - v"
        Curve
        {
            fit_curve a*x**b a,b
            legend sprintf("cartesian/CoviMAC order %3.1f", param1)
            file cartesian/CoviMAC/conv_glob_v
            colonnes 1 2
            style linespoints
        }
        Curve
        {
            fit_curve a*x**b a,b
            legend sprintf("cartesian/PolyMAC order %3.1f", param1)
            file cartesian/PolyMAC/conv_glob_v
            colonnes 1 2
            style linespoints
        }
        Curve
        {
            fit_curve a*x**b a,b
            legend sprintf("cartesian/VDF order %3.1f", param1)
            file cartesian/VDF/conv_glob_v
            colonnes 1 2
            style linespoints
        }
        Curve
        {
            fit_curve a*x**b a,b
            legend sprintf("non_conformal/CoviMAC order %3.1f", param1)
            file non_conformal/CoviMAC/conv_glob_v
            colonnes 1 2
            style linespoints
