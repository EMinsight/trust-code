Parameters {
	Title "cas test debit"
	Author "G.F."
	description "Calculs menes en VEF ou VDF"
	reference "moi_meme"
	TestCase .  test_opt.data exemple de debit impose
	TestCase .  test_opt2.data
	TestCase .  test_opt3.data
	TestCase .  Champ_front_debit.data
}
Chapter {
	Title "VEF test: calculated flow rate "
	# definition figure 0
	Table {
		nb_columns 2
		label  temps|debit
		line {
			legend calcul
			alias calcul
			file test_opt_pb_Debit.out
			last_line
			columns $1 (-$5)
			nb_columns_file 6
		}
		line {
			legend calcul avec champ_input
			alias calcul_avec_champ_input
			file test_opt2_pb_Debit.out
			last_line
			columns $1 (-$5)
			nb_columns_file 6
		}
		line {
			legend calcul avec champ_front_fonc_txyz
			alias calcul_avec_champ_front_fonc_txyz
			file test_opt3_pb_Debit.out
			last_line
			columns $1 (-$5)
			nb_columns_file 6
		}
		line {
			legend voulu
			alias voulu
			origin jdd
			values 10. 5.
		}
	}
	# definition figure 1
	Visu {
		Title "Visualization of the velocity field on boundary IN"
		description The velocity should be uniform even if the surfaces are different.
		mesh test_opt.lata IN
		vector test_opt.lata IN VITESSE ELEM
		normal3d -0.0543768 -0.677396 0.733606
		up3d -0.407612 0.685749 0.602993
		cycles  -1
		width 12cm
	}
	# definition figure 2
	Figure {
		Title "Test with with keyword champ_front_fonc_txyz"
		Title_figure " "
		labelX t
		labelY debit
		Include_Description_Curves 0
		width 10cm
		Curve {
			file test_opt3_pb_Debit.out
			columns 1 (-$5)
			style linespoints
		}
		Curve {
			origin ana
			fonction (1+4*x/10)
		}
	}
}
Chapitre {
	titre "VDF test: calculated flow rate"
	# definition figure 3
	Tableau {
		nb_colonnes 2
		label  temps|debit
		Ligne {
			legende Calculation with imposed flow rate
			fichier Champ_front_debit_pb_Debit.out
			derniere_ligne
			colonnes $1 (-$2)
			nb_colonnes_fichier 6
		}
		Ligne {
			legende Calculation with imposed velocity
			fichier testCL_pb_Debit.out
			derniere_ligne
			colonnes $1 (-$2)
			nb_colonnes_fichier 6
		}
		Ligne {
			legende voulu
			origine jdd
			valeurs 1.2 5.
		}
	}
}
