#!/bin/bash
# Recuperer une ancienne version du fichier source dans la base sccs :

if [ -f $rep_dev/.pcs ] 
then
   nom=`head -1 $rep_dev/.pcs`
   pcs=`tail -1 $rep_dev/.pcs`
else
   Erreur.ihm "Fichier $rep_dev/.pcs inexistant." 
fi
Recuperer.sccs $rep_dev/$nom
# On efface les fichiers contenant les 
rm -f $rep_dev/.$nom.*.old
rm -f $rep_dev/.pcs
# On ferme la fenetre du browser
kill -9 $pcs
