# PARALLEL OK 8 #
dimension 2
pb_conduction pbt1
pb_conduction pbt2
Domaine dom

# BEGIN MESH #
Mailler dom
{
    Pave Cavite
    {
        Origine 0. 0.
        Nombre_de_Noeuds 31 31
        Longueurs 2 2
    }
    {
        bord gauche X = 0  0 <= Y <= 2
        bord haut   Y = 2  0 <= X <= 2
        bord bas    Y = 0  0 <= X <= 2
        bord droite X = 2  0 <= Y <= 2
    }
}
# END MESH #

# BEGIN PARTITION
Decouper dom
{
    partitionneur metis { nb_parts 2 }
    nb_parts_tot 2
    Larg_joint 2
    Nom_Zones dom
}
Fin
END PARTITION #

# BEGIN SCATTER
Scatter dom.Zones dom
END SCATTER #

Scheme_euler_implicit sch
Read sch
{
    tinit 0
	nb_pas_dt_max 3
    dt_max 1
    facsec 1e30
    facsec_max 1e30
    seuil_statio 1e-3
    dt_impr 1e-10
    solveur implicite
    {
        solveur petsc cholesky { }
        seuil_convergence_implicite 1e30
    }
}

PolyMAC_P0P1NC dis

associate pbt1 dom
associate pbt2 dom
probleme_couple pb
Associate pb pbt1
Associate pb pbt2
Associate pb sch
Discretize pb dis

Read pbt1
{

    solide
    {
        rho      champ_uniforme 1 2000.0
        cp       champ_uniforme 1 3075.0
        lambda   champ_uniforme 1 0.5
    }

    conduction
    {
        diffusion   { }
        initial_conditions  { temperature  champ_uniforme  1  1000 }
        boundary_conditions
        {
            bas     paroi_adiabatique
            gauche  paroi_adiabatique
            droite  paroi_adiabatique
            haut    paroi_adiabatique
        }
        sources
        {
            terme_puissance_thermique_echange_impose
            {
                himp  champ_uniforme  1  1e6
                Text  champ_fonc_fonction pbt2 temperature 1 val
            }
        }
    }

    Post_processing
    {
        Probes
        {
            T1   grav temperature periode   0   point 1 1 1
            T1_A grav temperature periode 1e8 segment 500 0 1 2 1
            T1_B grav temperature periode 1e8 segment 500 1 0 1 2
        }
        format lml
        fields dt_post 1
        {
            temperature      elem
        }
    }
}

Read pbt2
{

    solide
    {
        rho      champ_uniforme 1 2000.0
        cp       champ_uniforme 1 3075.0
        lambda   champ_uniforme 1 0.5
    }

    conduction
    {
        diffusion   { }
        initial_conditions  { temperature  champ_uniforme  1  0 }
        boundary_conditions
        {
            bas     paroi_adiabatique
            gauche  paroi_adiabatique
            droite  paroi_adiabatique
            haut    paroi_adiabatique
        }
        sources
        {
            terme_puissance_thermique_echange_impose
            {
                himp  champ_uniforme  1  1e6
                Text  champ_fonc_fonction pbt1 temperature 1 val
            }
        }
    }

    Post_processing
    {
        Probes
        {
            T2   grav temperature periode   0   point 1 1 1
            T2_A grav temperature periode 1e8 segment 500 0 1 2 1
            T2_B grav temperature periode 1e8 segment 500 1 0 1 2
        }
        format lml
        fields dt_post 1
        {
            temperature      elem
        }
    }
}

Solve pb
End
