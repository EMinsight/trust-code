#include "defineh_inc"
      real XM,YM,ZM
      integer INDMAIL,IPRES,NMAPRO1,NMAPRO2   
#ifdef FIELD_
	real XMP,YMP,ZMP
	integer IVALP,IPREP
#endif
      COMMON XM(NIMAX),YM(NJMAX),ZM(NKMAX)                               
      COMMON INDMAIL(NIMAX-1,NJMAX-1,NKMAX-1)           
      COMMON IPRES(NIMAX-1,NJMAX-1,NKMAX-1)           
      COMMON NMAPRO1(64)  
      COMMON NMAPRO2(64)  
#ifdef FIELD_
      COMMON XMP(2*NIMAX-2),YMP(2*NJMAX-2),ZMP(2*NKMAX-2)	
      COMMON IVALP(2*NIMAX-2,2*NJMAX-2,2*NKMAX-2)           
      COMMON IPREP(2*NIMAX-2,2*NJMAX-2,2*NKMAX-2)     
#endif

