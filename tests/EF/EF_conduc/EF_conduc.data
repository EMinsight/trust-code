# Conduction 2D : Diverses conditions limites thermiques #
# PARALLEL OK 8 #
dimension 2
EcritureLectureSpecial 0
Pb_conduction pb
Domaine dom
# DEBUT MAILLAGE #
Mailler dom
{
	Pave Cavite 
 	{
		Origine 0. 0.
		Nombre_de_Noeuds  6 6
		Longueurs 1. 1.
	}
	{
		Bord Gauche X = 0. 0. <= Y <= 1.
		Bord Haut   Y = 1. 0. <= X <= 1.
		Bord Bas    Y = 0. 0. <= X <= 1.
		Bord Droit  X = 1. 0. <= Y <= 1.
	}
} 
 trianguler_h dom
# #
ecrire_fichier dom dom.geom
# FIN MAILLAGE #
# DEBUT DECOUPAGE
Decouper dom
{
	Partitionneur tranche { tranches 2 1 }
	Larg_joint 1
	Nom_Zones DOM
}
Fin
FIN DECOUPAGE #

# DEBUT LECTURE
Scatter DOM.Zones dom
FIN LECTURE #

EF dis

Schema_Euler_explicite sch
Lire sch
{
	tinit 0
	tmax 300.
	dt_min 0.00001
	dt_impr 1
	dt_sauv 100
	seuil_statio 1.e-8
nb_pas_dt_max 1000
}

Solide sol
Lire sol
{
	rho Champ_Uniforme 1 2
	lambda Champ_Uniforme 1 0.01
	Cp Champ_Uniforme 1 0.5
}
Associer pb dom
Associer pb sch
Associer pb sol

Discretiser pb dis

Lire pb
{
	Conduction
	{
		diffusion { }
		conditions_initiales {
			temperature Champ_fonc_xyz dom  1 x*0 
			}
		conditions_limites {
			Haut paroi_adiabatique
			Bas paroi_adiabatique
			Droit paroi_temperature_imposee
                                Champ_Front_Uniforme 1 1.
			Gauche paroi_temperature_imposee
                                Champ_Front_Uniforme 1 0.
		}
	}

	Postraitement
	{
		Sondes
		{
			sonde temperature periode 5. plan 10 10 0. 0.  0. 1. 1. 0.
			seg temperature periode 5. segment 10  0. 0.   1. 0.
		}
		Champs dt_post  1e5
		{
			temperature som
		}
	}
	Postraitements
	{
	lata { 
		format lata
		 Champs dt_post 1e5
                {
                        temperature som
                }

	}
	}
}

Resoudre pb

Fin
