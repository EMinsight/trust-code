# Hydraulique 3D VEF #
# PARALLEL OK 3 #
dimension 3
Domaine dom

# BEGIN MESH #
Mailler dom
{
Pave Cavite
        {
                Origine 0.  0. 0.
                Nombre_de_Noeuds 5 5 5
                Longueurs 1 1 1
        }
        {
                Bord paroi   X = 0.   0. <= Y <= 1.    0. <= Z <= 1. 
                Bord paroi  X = 1.  0. <= Y <= 1.       0. <= Z <= 1. 
                Bord paroi   Y =  0.  0. <= X <= 1.      0. <= Z <= 1. 
                Bord paroi   Y =  1.  0. <= X <= 1.    0. <= Z <= 1. 
		Bord paroi Z = 0  0. <= X <= 1.  0. <= Y <= 1.
		Bord paroi Z = 1  0. <= X <= 1.  0. <= Y <= 1.
        }
} 
Tetraedriser_homogene dom
Transformer dom
x+x*(1-x)*y*(1-y)*(0.5-y)
y+x*(1-x)*y*(1-y)*(0.5+x)
z
# END MESH #

# BEGIN PARTITION
Partition dom
{
	Partitionneur Metis { Nb_parts 3 }
	Larg_joint 2
	Nom_Zones DOM
}
Fin
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #

VEFPreP1b dis
Lire dis { P0 P1 Pa }

Schema_Euler_Explicite sch
Lire sch 
{
	nb_pas_dt_max 5
        tinit 0.
        tmax 0.1
        dt_min 1.e-6
        dt_max 1.e-2
	dt_start dt_calc
        dt_impr 0.1
        dt_sauv 20.
        seuil_statio 1.e-8  
        facsec 0.5
}
Fluide_Incompressible fluide
Lire fluide
{
	mu Champ_Uniforme	1 1.e-6
	rho Champ_Uniforme	1 1.
}

Pb_Hydraulique pb
Associate pb dom
Associate pb sch
Associate pb fluide

Discretize pb dis
# Debog pb seq faces 1.e-6 0 #
Lire pb
	{
	Navier_Stokes_standard
		{
		dt_projection 1. 1.e-12

		Solveur_Pression petsc cholesky { }

		solveur_bar Cholesky { }

                convection { EF filtrer_resu 0 antisym 0 transporte_bar 0 transportant_bar 0 }
                diffusion  {  }
			
		conditions_initiales
			{
                        vitesse Champ_fonc_xyz dom 3  x-0.5 0.5-y 0
			}
		boundary_conditions
			{
			paroi Frontiere_Ouverte_Vitesse_Imposee 
				Champ_front_fonc_xyz 3 x-0.5 0.5-y 0
			}
	         }

	Postraitements
        	{
		lata	
		{
		fichier Tourbillon3DConv
		format lata  
                Sondes
                	{ 
                        sonde_vit1  vitesse periode 0.01 points 1 0.5 0.5 0.5 
			}
		Champs dt_post 0.1
			{
			vitesse elem
			pression elem
			pression som
			}
		}
		lml
		{
		fichier Tourbillon3DConv
		format lml  
		Champs dt_post 0.1
			{
			vitesse elem
			pression elem
			pression som
			}
		}
		}
	}

Solve pb
Fin
