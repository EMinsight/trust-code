#!/bin/bash
ETUDE_TRUST=`QUELLE_ETUDE.ihm`
export ETUDE_TRUST
cd $ETUDE_TRUST
NOM=`cat $ETUDE_TRUST/.NOM_DATA`
export NOM
if [ -f $NOM.lata ]
then
   fichier=$NOM.lata
elif [ -f $NOM.lml ]
then
   fichier=$NOM.lml      
else
   fichier=""
   echo "No file $NOM.lata or $NOM.lml available."
   echo "Please check your data file in the Postraitement description."
   eval $FIN
fi
visit -o $fichier
sleep 3

