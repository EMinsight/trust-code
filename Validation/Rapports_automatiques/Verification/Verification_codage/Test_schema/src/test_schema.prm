Parameters {
	Title "Time scheme test"
	Author "fauchet "
	Description "On regarde l'ordre de convergence des differents schemas en temps"
	# Run test cases (post_run script is run after the calculation if found in the test case directory):
	TestCase . test_schema_1.e-3
	TestCase . test_schema_1.e-2
	TestCase . test_schema_1.e-1
	TestCase . test_schema_5.e-1
	TestCase . test_schema_1.e0
	TestCase . test_stab_schema_2.e0
	TestCase . test_stab_schema_3.e0
	TestCase . test_stab_schema_4.e0
	TestCase . test_stab_schema_5.e0	
	TestCase . test_stab_schema_10.e0
	# Run scripts before building PDF report:
	Prerequisite sh ./get_erreur
	Prerequisite sh ./get_stab
}

Chapter {
	Figure
	{
		labelx dt
		labely erreur
		logx 
		logy
		Title "courbe de convergence"
		Description " On voit clairement que le schema explicite est d'ordre 1, que le RK2 est d'ordre 2, que le RK 3 d'ordre 3. Le RK4 semble etre d'ordre 2.
		legend bottom
		Include_Description_Curves 0
		Curve {
			file erreur
			columns $1 $2
			legend explicite
			style linespoints
		}
		Curve {
			file erreur
			columns $1 $3
			legend RK2
			style linespoints
		}
		Curve {
			file erreur
			columns $1 $4
			legend RK3
			style linespoints
		}
		Curve {
			file erreur
			columns $1 $5
			legend RK4
			style linespoints
		}
		Curve {
			fonction x*1e-1
			legend ordre 1
			style lines
		}
		Curve {
			fonction x*x*1e-2
			legend ordre 2
			style lines
		}
		Curve {
			fonction x*x*x*1e-2
			legend ordre 3
			style lines
		}
	}	
	Table {
     		nb_columns 4
		Description "Check that temperature stay between -100 and 100"
		label explicite|RK2|RK3|RK4
		line { 
			file stab
			legend dt_max
		}
	}
}
