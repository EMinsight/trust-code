# Thermohydraulique 2D : Couplage Boussinesq et schema quick #
# Test du postraitement sur un domaine d interpolation different du domaine de calcul #
# PARALLEL NOT #
dimension 2
Pb_Thermohydraulique pb
Domaine dom
Domaine dom_post1
Domaine dom_post2
# BEGIN MESH #
Mailler dom
{
	Pave Cavite 
		{
		Origine 0. 0.
     		Nombre_de_Noeuds 11 11
		Longueurs 0.01 0.01
		}
		{
		Bord Gauche X = 0.  0. <= Y <= 0.01
		Bord Haut Y = 0.01  0. <= X <= 0.01
		Bord Bas Y = 0.     0. <= X <= 0.01
		Bord Droit X = 0.01 0. <= Y <= 0.01
		}
}

Mailler dom_post1
{
	Pave Cavite 
		{
		Origine 0.005 0.
     		Nombre_de_Noeuds 6 6
		Longueurs 0.01 0.01
		}
		{
		Bord Gauche1 X = 0.005  0. <= Y <= 0.01
		Bord Haut1 Y = 0.01  0.005 <= X <= 0.015
		Bord Bas1 Y = 0.     0.005 <= X <= 0.015
		Bord Droit1 X = 0.015 0. <= Y <= 0.01
		}
}

Mailler dom_post2
{
	Pave Cavite 
		{
		Origine 0. 0.005
     		Nombre_de_Noeuds 6 6
		Longueurs 0.01 0.01
		}
		{
		Bord Gauche2 X = 0.  0.005 <= Y <= 0.015
		Bord Haut2 Y = 0.015  0. <= X <= 0.01
		Bord Bas2 Y = 0.005     0. <= X <= 0.01
		Bord Droit2 X = 0.01 0.005 <= Y <= 0.015
		}
}

# END MESH #
# BEGIN PARTITION
Partition dom
{
	Partitionneur tranche { tranches 2 1 }
	Larg_joint 2
	Nom_Zones dom
}
Partition dom_post1
{
	Partitionneur tranche { tranches 2 1 }
	Larg_joint 2
	Nom_Zones dom_post1
}

Partition dom_post2
{
	Partitionneur tranche { tranches 2 1 }
	Larg_joint 2
	Nom_Zones dom_post2
}

Fin
END PARTITION #

# BEGIN SCATTER
Scatter dom.Zones dom
Scatter dom_post1.Zones dom_post1
Scatter dom_post2.Zones dom_post2
END SCATTER #

VDF dis

Schema_Euler_explicite sch
Lire sch
{
	tinit 0.
	tmax 2.5
	dt_min 0.01
	dt_max 0.01
	dt_impr 0.01
	dt_sauv 100
	seuil_statio 1.e-8
}

Fluide_Incompressible fluide
Lire fluide
{
	mu Champ_Uniforme 1 1.85e-5
	rho Champ_Uniforme 1 1.17
	lambda Champ_Uniforme 1 0.0262 
	Cp Champ_Uniforme 1 1006
	beta_th Champ_Uniforme 1 3.41e-3
}

Champ_Uniforme gravite
Lire gravite 2 0 -9.81

Associate fluide gravite
Associate pb dom
Associate pb sch
Associate pb fluide

Discretize pb dis

Lire pb
{
	Navier_Stokes_standard
	{
		projection_initiale 0
		dt_projection 1. 1.e-08
		solveur_pression GCP { 
			precond_nul
			seuil 1.000000e-16 
			impr
		}
		convection { quick }
		diffusion { }
		sources { boussinesq_temperature { T0 5. } }
		conditions_initiales {
			vitesse Champ_Uniforme 2 0. 0.
			}
		boundary_conditions {
			Haut paroi_fixe
			Bas paroi_fixe
			Gauche paroi_fixe
			Droit paroi_fixe
		}
	}
	Convection_Diffusion_Temperature
	{
		diffusion { }
		convection { quick }
		boundary_conditions 
		{
			Haut paroi_adiabatique
			Bas paroi_adiabatique
 			Gauche paroi_temperature_imposee Champ_Front_Uniforme 1 10.
			Droit  paroi_temperature_imposee Champ_Front_Uniforme 1 0.  
		}
		conditions_initiales { Temperature Champ_Uniforme 1 5. }
	}

	Postraitements
	{
	      first_post
	      {
		Sondes
		{
			sonde_V vitesse periode 0.01 segment 10 0.0005 0.005 0.0095 0.005
			sonde_pression pression periode 0.01 points 1 0.0045 0.0045
			sonde_vitesse vitesse periode 0.01 points 1 0.0045 0.0045
			sonde_temp temperature periode 0.01 points 1 0.0045 0.0045
			sonde_p pression periode 0.1 plan 10 10 0.0005 0.0005 0.0095 0.0005 0.0005 0.0095

		}
		Format lata
		Champs dt_post 0.1
		{
			pression_pa elem
			pression_pa som
			vitesse elem
			vitesse som
			temperature elem
			temperature som
			vorticite elem
		}
	     }
	    second_post
	    {
	      Sondes
	      {
		  sonde_temp_interp temperature_elem_dom_post2 periode 0.01 points 1 0.008 0.008
	      }
	      fichier VAHL_DAVIS_post
	      Definition_champs 
		{
		 temperature_elem_dom_post1 Interpolation {
		        domaine dom_post1
		  	localisation elem
			source refChamp { Pb_champ pb temperature }
		  	}
		 temperature_elem_dom_post2 Interpolation {
		        domaine dom_post2
		  	localisation elem
			sources_reference { temperature_elem_dom_post1 } 
		  	}	
			
		}
	     
	      Domaine dom_post2 
	      Champs dt_post 0.1 {
		  temperature_elem_dom_post2
	      }
	    }
	}
}

Solve pb
Fin
