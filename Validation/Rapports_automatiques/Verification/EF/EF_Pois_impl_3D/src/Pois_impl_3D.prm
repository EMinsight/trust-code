Parametres {
	Titre "Poiseuille en EF"
	Description "Calculs menés en EF, delta p impos\'e"
	Auteur "G.F."
	CasTest . Pois_impl_3D.data
}

Chapitre {
	Titre "R\'esultats"
	Description ""
	 visu {
	titre "vitesse_x"
	mesh ./Pois_impl_3D.lata dom
	pseudocolor ./Pois_impl_3D.lata dom VITESSE_X SOM
}
visu {
	titre "Erreurs"
	description "on trace la pression moins la solution, ainsi que la vitesse moins la solution"
	origine comparaison
	mesh ./Pois_impl_3D.lata dom
	instruction_visit DefineVectorExpression("erreur_SOM_dom","VITESSE_SOM_dom-{(coord(dom)[1])*(0.22-coord(dom)[1]),0,0}")
	instruction_visit DefineScalarExpression("sol_p_ELEM_dom","PRESSION_PA_ELEM_dom*0-(coord(dom)[0]-0.1)*6")
	instruction_visit DefineScalarExpression("erreur_p_ELEM_dom","PRESSION_PA_ELEM_dom-sol_p_ELEM_dom")
	vector ./Pois_impl_3D.lata dom erreur SOM
	pseudocolor ./Pois_impl_3D.lata dom erreur_p ELEM
}
}





