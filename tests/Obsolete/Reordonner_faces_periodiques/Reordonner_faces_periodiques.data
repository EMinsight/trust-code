# Test de la syntaxe de Corriger_frontiere_periodique #
# PARALLEL OK 8 #
dimension 3

Pb_hydraulique pb
Domaine dom

# BEGIN MESH #
Mailler dom
{
	Pave Entree 
	{
	Origine 0. 0. 0.
	Nombre_de_Noeuds 9 9 9
	Longueurs 6.283185307 6.283185307 6.283185307
	}
	{
	Bord periox X = 0. 0. <= Y <= 6.283185307 0. <= Z <= 6.283185307 
	Bord periox X = 6.283185307 0. <= Y <= 6.283185307 0. <= Z <= 6.283185307 
	Bord perioy Y = 0. 0. <= X <= 6.283185307 0. <= Z <= 6.283185307 
	Bord perioy Y = 6.283185307 0. <= X <= 6.283185307 0. <= Z <= 6.283185307 
	Bord perioz Z = 0. 0. <= X <= 6.283185307 0. <= Y <= 6.283185307 
	Bord perioz Z = 6.283185307 0. <= X <= 6.283185307 0. <= Y <= 6.283185307 	

	}
}
Tetraedriser dom
Corriger_frontiere_periodique { domaine dom bord periox }
Corriger_frontiere_periodique { domaine dom bord perioy }
Corriger_frontiere_periodique { domaine dom bord perioz }

# END MESH #
# BEGIN PARTITION
Partition dom
{
	Partition_tool tranche { tranches 2 1 1 }
	zones_name DOM
	Periodique 3 periox perioy perioz
        Larg_joint 2
}
End
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #

VEFPreP1B dis

Scheme_euler_explicit sch
Read sch
{
	tinit 0.
	tmax 2.0
	dt_min 0.00001
	dt_max 0.1
	dt_impr 0.01
	dt_sauv 0.5
	seuil_statio 1.e-8
	nb_pas_dt_max 5
}

Fluide_Incompressible air
Read air
{
	mu Champ_Uniforme 1 2e-20
	rho Champ_Uniforme 1 2
}

Associate pb dom
Associate pb sch
Associate pb air

Discretize pb dis

Read pb
{
	Navier_Stokes_standard

	{
		solveur_pression Gcp { precond ssor { omega 1.5 }
                                        seuil 1.e-8
                                        impr
                                        }   	
		convection { EF_stab { } }
		diffusion { }
		initial_conditions {  
               	vitesse champ_uniforme 3 1 1 1
		}
		boundary_conditions {
                   periox periodique 
                   perioy periodique 
                   perioz periodique 
		}
        }
	Post_processing
	{
		Probes
		{
			pression pression periode 0.001 points 1 3.14 3.14 3.14
			vitesse vitesse  periode 0.001 points 1 3.14 3.14 3.14
			profil vitesse  periode 0.001 segment 10 0. 3.14 3.14 6.28 3.14 3.14
		}
		fields dt_post 5.
		{
			vitesse elem	
		}
	}
}

Solve pb
End
