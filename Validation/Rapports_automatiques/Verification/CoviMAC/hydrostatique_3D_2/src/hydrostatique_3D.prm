Chapter
{
    Title "PolyMAC"
    Description "Test du gradient de pression en non conforme"
    
    visu
    {
        # Width 11cm,viewport= 0 0 1024 800,clip
        # instruction_visit v=GetView3D()
        normal3D 0. 0. 1.
        up3D 0 0 0
        Title "Mesh : PolyMAC"
        # mesh cartesian.med mesh
        mesh        droite.lata dom
        pseudocolor droite.lata dom PRESSION ELEM
        vector      droite.lata dom VITESSE ELEM
        Description " A remplir "
    }


    Tableau
    {
        Titre "Comparaison theorie/numerique"
        nb_colonnes 2
        # label "Erreur P" | "Erreur v"
        label \latex_( $\Vert P \Vert_{\infty}$ \latex_) |  \latex_( $\Vert \vec{v} \Vert_{\infty}$ \latex_)
        Ligne
        {
            Legende "droite"
            fichier droite_erreurs.txt
        }
        Ligne
        {
            Legende "gauche"
            fichier gauche_erreurs.txt
        }
        Ligne
        {
            Legende "devant"
            fichier devant_erreurs.txt
        }
        Ligne
        {
            Legende "derriere"
            fichier derriere_erreurs.txt
        }
        Ligne
        {
            Legende "haut"
            fichier haut_erreurs.txt
        }
        Ligne
        {
            Legende "bas"
            fichier bas_erreurs.txt
        }
    }
