# Conduction 2D : Diverses conditions limites thermiques #
# PARALLEL OK 8 #
dimension 2

Pb_conduction pb
Domaine dom
# BEGIN MESH #
Mailler dom
{
	Pave Cavite 
 	{
		Origine 0. 0.
		Nombre_de_Noeuds 11 11
		Longueurs 1. 1.
	}
	{
		Bord Gauche1 X = 0. 0. <= Y <= 0.5
		Bord Gauche2 X = 0. 0.5 <= Y <= 1.
		Bord Haut   Y = 1. 0. <= X <= 1.
		Bord Bas    Y = 0. 0. <= X <= 1.
		Bord Droit  X = 1. 0. <= Y <= 1.
	}
} 


# END MESH #
# BEGIN PARTITION
Partition dom
{
	Partition_tool tranche { tranches 2 1 }
	Larg_joint 1
	zones_name DOM
}
End
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #

VDF dis

Scheme_euler_explicit sch
Read sch
{
	tinit 0
	tmax 300.
	dt_min 0.00001
	dt_max 0.1
	dt_impr 5
	dt_sauv 100
	seuil_statio 1.e-8
}

Solide sol
Read sol
{
	rho Champ_Uniforme 1 2
	lambda Champ_Uniforme 1 0.01
	Cp Champ_Uniforme 1 0.5
}
Associate pb dom
Associate pb sch
Associate pb sol

Discretize pb dis

Read pb
{
	Conduction
	{
		diffusion { }
		initial_conditions {
			temperature Champ_Uniforme 1 0. 
			}
		boundary_conditions {
			Haut paroi_adiabatique
			Bas paroi_temperature_imposee
			        Champ_Front_Uniforme 1 5.
			Droit paroi_flux_impose  
			      Champ_Front_Uniforme 1 0.1
			Gauche1 paroi_echange_global_impose 
			       h_imp Champ_Front_Uniforme 1 0.1 
			       T_ext Champ_Front_Uniforme 1 0.
			Gauche2 echange_couplage_thermique { temperature_paroi Champ_uniforme 1 0 flux_paroi Champ_uniforme 1 0.1 }
		}
	}

	Post_processing
	{
		Definition_champs
		{
			flux_Haut Reduction_0D { 
				methode sum 
				source Interpolation {
					domaine dom_boundaries_Haut
					localisation elem
					source Morceau_Equation {
						type operateur numero 0 option flux_bords
						source refChamp { Pb_champ pb temperature }
					}
				}
         		}
			flux_Gauche1 Reduction_0D { 
				methode sum 
				source Interpolation {
					domaine dom_boundaries_Gauche1
					localisation elem
					source Morceau_Equation {
						type operateur numero 0 option flux_bords
						source refChamp { Pb_champ pb temperature }
					}
				}
         		}
			flux_Gauche2 Reduction_0D { 
				methode sum 
				source Interpolation {
					domaine dom_boundaries_Gauche2
					localisation elem
					source Morceau_Equation {
						type operateur numero 0 option flux_bords
						source refChamp { Pb_champ pb temperature }
					}
				}
         		}
			flux_Droit Reduction_0D { 
				methode sum 
				source Interpolation {
					domaine dom_boundaries_Droit
					localisation elem
					source Morceau_Equation {
						type operateur numero 0 option flux_bords
						source refChamp { Pb_champ pb temperature }
					}
				}
         		}
			flux_Bas Reduction_0D { 
				methode sum 
				source Interpolation {
					domaine dom_boundaries_Bas
					localisation elem
					source Morceau_Equation {
						type operateur numero 0 option flux_bords
						source refChamp { Pb_champ pb temperature }
					}
				}
         		}
		}
		Probes
		{
			sonde temperature periode 5. points 1 0.45 0.45
			flux_Haut flux_Haut periode 5 numero_elem_sur_maitre 0
			flux_Gauche1 flux_Gauche1 periode 5 numero_elem_sur_maitre 0
			flux_Gauche2 flux_Gauche2 periode 5 numero_elem_sur_maitre 0
			flux_Droit flux_Droit periode 5 numero_elem_sur_maitre 0
			flux_Bas flux_Bas periode 5 numero_elem_sur_maitre 0
		}
		fields dt_post 30.
		{
			temperature elem
			temperature som
		}
	}
}
Imprimer_flux dom { Haut }
Solve pb
End
