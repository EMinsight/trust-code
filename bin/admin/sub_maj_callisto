#!/bin/bash 
#SBATCH -J mise_a_jour_TRUST_arch
#SBATCH -p slim
#SBATCH --qos=normal
#SBATCH -t 720
#SBATCH -o ~/myjob_callisto.%j.o
#SBATCH -e ~/myjob_callisto.%j.e
#SBATCH -n 1
cd $SLURM_SUBMIT_DIR
[ -f ld_env.sh ] && . ./ld_env.sh # To load an environment file if necessary
srun -n 1 ./mise_a_jour_TRUST_arch -rsync 1>~/CR_callisto 2>&1
