# Thermohydraulique 2D : reprise calcul #
# PARALLEL OK 2 #
dimension 2
Pb_Thermohydraulique pb
Domaine dom
# BEGIN MESH #
Mailler dom
{
    Pave Cavite
    {
        Origine 0. 0.
        Nombre_de_Noeuds 11 11
        Longueurs 0.01 0.01
    }
    {
        Bord Gauche X = 0. 0. <= Y <= 0.01
        Bord Haut Y = 0.01 0. <= X <= 0.01
        Bord Bas Y = 0. 0. <= X <= 0.01
        Bord Droit X = 0.01 0. <= Y <= 0.01
    }
}

# END MESH #
# BEGIN PARTITION
Partition dom
{
    Partition_tool tranche { tranches 2 1 }
    Larg_joint 2
    zones_name DOM
}
End
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #

VDF dis
Scheme_euler_explicit sch
Read sch
{
    tinit 0.002
    dt_min 0.001
    dt_max 0.001
    nb_pas_dt_max 2
    seuil_statio 1.e-8
}

Associate pb dom
Associate pb sch
Discretize pb dis

Read pb
{
       
fluide_incompressible {
    gravite champ_uniforme 2 0 -9.81

mu Champ_Uniforme 1 1.85e-5
    rho Champ_Uniforme 1 1.17
    lambda Champ_Uniforme 1 0.0262
    Cp Champ_Uniforme 1 1006
    beta_th Champ_Uniforme 1 3.41e-3
}

Navier_Stokes_standard
    {
        solveur_pression GCP {
            precond ssor { omega 1.5 }
            seuil 1.e-12
            impr
        }
        convection { Quick }
        diffusion { }
        sources { boussinesq_temperature { T0 5. } }
        initial_conditions {
            vitesse Champ_Uniforme 2 0. 0.
        }
        boundary_conditions {
            Haut paroi_fixe
            Bas paroi_fixe
            Gauche paroi_fixe
            Droit paroi_fixe
        }
    }
    Convection_Diffusion_Temperature
    {
        diffusion { }
        convection { quick }
        boundary_conditions
        {
            Haut paroi_adiabatique
            Bas paroi_adiabatique
            Gauche paroi_temperature_imposee Champ_Front_Uniforme 1 10.
            Droit  paroi_temperature_imposee Champ_Front_Uniforme 1 0.
        }
        initial_conditions { Temperature Champ_Uniforme 1 5. }
    }

    Post_processing
    {
        fields dt_post 1 { pression elem }
    }
    # sauvegarde binaire sauve #
    reprise binaire ../Extension/sauve
}
Solve pb
End

 