Parameters
{
    Title "Cavite entrainee - Re = 400"
    Description ""
    Author "Y.G."
    TestCase cartesian     "jdd.data"
    TestCase triangle      "jdd.data"
    TestCase non_conformal "jdd.data"
}

Chapter
{
    Title "Maillages et premiere composante de vitesse"
    Description ""
    
    visu
    {
        normal3D 0. 0. 1.
        up3D 0.161316 0.944928 -0.28476
        Title "Maillage cartesien"
        mesh        cartesian/jdd.lata dom
        pseudocolor cartesian/jdd.lata dom VITESSE_X ELEM
        Description ""
    }

    visu
    {
        normal3D 0. 0. 1.
        up3D 0.161316 0.944928 -0.28476
        Title "Maillage de triangles"
        mesh        triangle/jdd.lata dom
        pseudocolor triangle/jdd.lata dom VITESSE_X ELEM
        Description ""
    }

    visu
    {
        normal3D 0. 0. 1.
        up3D 0.161316 0.944928 -0.28476
        Title "Maillage non conforme"
        mesh        non_conformal/jdd.lata dom
        pseudocolor non_conformal/jdd.lata dom VITESSE_X ELEM
        Description ""
    }
}
Chapter
{
    Title "Comparaisons a une solution de reference"
    Description "La reference est dans le src"
    Figure
    {
        incluredesccourbes 0
        Titre "Comparaison"
        Dimension 2
        LabelX "position"
        LabelY "velocity"
        description "Reference : The lid-driven square cavity flow: numerical solution with a 1024 x 1024 grid, Marchi et al., Journal of the Brazilian Society of Mechanical Sciences and Engineering, 2009"
        # width 7cm
        
        Courbe
        {
            Legende "vx, reference"
            fichier ./ref_vx.txt
            Style p
            TypePoints 7
            TypeLigne 3
        }
        Courbe
        {
            Legende "vy, reference"
            fichier ./ref_vy.txt
            Style p
            TypePoints 7
            TypeLigne 1
        }
        Courbe
        {
            Legende "vx, cartesian mesh, PolyMAC"
            Origine "Trio_U"
            segment ./cartesian/jdd.data vx
            Style l
            colonnes ($1) ($2)
            TypeLigne -1
        }
        Courbe
        {
            Legende "vy, cartesien mesh, PolyMAC"
            Origine "Trio_U"
            segment ./cartesian/jdd.data vy
            Style l
            colonnes ($1) ($3)
            TypeLigne 2
        }
        Courbe
        {
            Legende "vx, triangle mesh, PolyMAC"
            Origine "Trio_U"
            segment ./triangle/jdd.data vx
            Style l
            colonnes ($1) ($2)
            TypeLigne 4
        }
        Courbe
        {
            Legende "vy, triangle mesh, PolyMAC"
            Origine "Trio_U"
            segment ./triangle/jdd.data vy
            Style l
            colonnes ($1) ($3)
            TypeLigne 5
        }
        Courbe
        {
            Legende "vx, non conformal mesh, PolyMAC"
            Origine "Trio_U"
            segment ./non_conformal/jdd.data vx
            Style l
            colonnes ($1) ($2)
            TypeLigne 6
        }
        Courbe
        {
            Legende "vy, non conformal mesh, PolyMAC"
            Origine "Trio_U"
            segment ./non_conformal/jdd.data vy
            Style l
            colonnes ($1) ($3)
            TypeLigne 7
        }

    }
}
