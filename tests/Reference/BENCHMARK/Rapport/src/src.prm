Parametres {
	Titre "Basic parallel benchmarking"
	Description ""
	Auteur "Pierre LEDAC (CS)"	
	VersionTrio_U "1.6.2"	
	Reference "See data files into \$TRUST_ROOT/tests/Reference/BENCHMARK directory"
}

Chapitre {
	Titre "Evolution of mean CPU time-step for a VDF calculation"
	Figure {
		Titre "Evolution of the mean CPU time-step (sequential calculation)"
		LabelX "Days ago"
		LabelY "Time step CPU [s]"
		RangeX -400. 0.
		RangeY 0. 16.
		InclureDescCourbes 0
		Courbe { 
			Legende "callisto"
			Style points
			Fichier callisto.cpu
			colonnes $1 $3
		}
		Courbe { 
			Legende "callisto-openmpi"
			Style points
			Fichier callisto-openmpi.cpu
			colonnes $1 $3
		}
		Courbe { 
			Legende "curie-ccrt"
			Style points
			Fichier curie-ccrt.cpu
			colonnes $1 $3
		}
		Courbe { 
			Legende "airain"
			Style points
			Fichier airain.cpu
			colonnes $1 $3
		}	
		Courbe { 
			Legende "ceres2"
			Style points
			Fichier ceres2.cpu
			colonnes $1 $3
		}	
		Courbe {
			Legende "Acceptance limit"
			Style lines
			Fonction 8
		}
	}
	Figure {
		Titre "Evolution of the speedup efficiency (4 processors calculation)"
		LabelX "Days ago"
		LabelY "Efficiency"
		RangeX -400. 0.
		RangeY 0. 2.
		InclureDescCourbes 0
		Courbe { 
			Legende "callisto"
			Style points
			Fichier callisto.cpu
			colonnes $1 $8
		}
		Courbe { 
			Legende "callisto-openmpi"
			Style points
			Fichier callisto-openmpi.cpu
			colonnes $1 $8
		}
		Courbe { 
			Legende "curie-ccrt"
			Style points
			Fichier curie-ccrt.cpu
			colonnes $1 $8
		}		
		Courbe { 
			Legende "airain"
			Style points
			Fichier airain.cpu
			colonnes $1 $8
		}
		Courbe { 
			Legende "ceres2"
			Style points
			Fichier ceres2.cpu
			colonnes $1 $8
		}
		Courbe {
			Legende "Acceptance limit"
			Style lines
			Fonction 0.6
		}				
	}
}

