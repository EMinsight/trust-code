# Hydraulique 2D laminaire avec "trou" dans le maillage #
# PARALLEL OK 4 #
dimension 2
Pb_conduction pb
Domaine dom
# BEGIN MESH #
Mailler dom
{
    Pave Cavite
    {
        Origine 0. 0.
        Nombre_de_Noeuds 5 2
        Longueurs 4. 1.
    }
    {
        Bord Gauche X = 0. 0. <= Y <= 1.
        Bord Haut   Y = 1. 0. <= X <= 4.
        Bord Bas    Y = 0. 0. <= X <= 4.
        Bord Droit  X = 4. 0. <= Y <= 1.
    }
}

Postraiter_domaine
{
    fichier mesh.lata
    format lata
    domaine dom
}

# END MESH #
# BEGIN PARTITION
Partition dom
{
    Partition_tool tranche { tranches 2 1 }
    Larg_joint 1
    zones_name DOM
}
End
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #

Sous_zone sz1
Associate sz1 dom
Read sz1 {
    rectangle
    Origine 0.0 0.0
    Cotes 2.0 1.0
}
Sous_zone sz2
Associate sz2 dom
Read sz2 {
    Rectangle
    Origine 2. 0.0
    Cotes 2.0 1.0
}


VDF dis

/* Scheme_euler_implicit sch */
Scheme_euler_explicit sch
Read sch
{
    tinit 0
    tmax 1.0
    nb_pas_dt_max 0
}

Associate pb dom
Associate pb sch
Discretize pb dis

Read pb
{
       
solide {
     rho Champ_Uniforme 1 50
    lambda Champ_Uniforme 1 0.01
    Cp Champ_Uniforme 1 0.5
}



Conduction
    {
        diffusion { }
        initial_conditions {

            temperature Champ_Fonc_Fonction_txyz_Morceaux pb rho 1
            {
                defaut 3000.0
                sz1 val+3
                sz2 val*2+2
            }

        }

        boundary_conditions {
            Haut paroi_adiabatique
            Bas  paroi_adiabatique
            Droit paroi_adiabatique
            Gauche paroi_adiabatique
        }
    }

    Post_processing
    {
        definition_champs
        {
            ma_comparaison Transformation
            {
                methode formule expression 1 temperature_natif_dom+10.0
                source refChamp { Pb_champ pb temperature }
            }
        }
        format lata
        fields dt_post 1.
        {
            ma_comparaison
            temperature elem
        }
    }


}

Solve pb
End

 