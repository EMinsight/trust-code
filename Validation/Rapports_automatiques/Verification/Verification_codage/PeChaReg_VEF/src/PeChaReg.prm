Parameters {
	Title "verification des pertes de charge regulieres  en VEF"
	Description "Calculs menes en VEF"
	Author "P.Q. +  G.F."
	TestCase "2D_perio" "test.data"
	TestCase "2D_sym" "test.data"
	TestCase "3D_perio" "test.data"
	TestCase "3D_sym" "test.data"
	TestCase "3D_sym_inout" "test.data"
	TestCase "3D_sym_inout" "test_poreux.data"
	TestCase "3D_sym_inout" "test_rotation.data"	
	IncludeData 1
}

Chapter {
	Title "Evolution de la  vitesse"
	Description "Dp/Dy = 0.5.rho.lambda(Re).U2/Dh "
	Description " "
	Description "Dp/Dy = 1 "
	Description "rho = 5 (solution independante de rho)"
	Description "mu = 4"
	Description "Dh = 2"
	Description " "
	Description " Vitesse ciblee : U = 2.Dh2/mu = 2"
Figure {
        Title "Evolution temporelle de la vitesse"
	Dimension 2
	RangeY 1.95 2.01
	LabelX "Temps (s)"
	LabelY "Vitesse (m/s)"

	Curve {
		legend "2D perio"
		origin "Trio_U"
		file 2D_perio/test_VIT.son
	Style linespoints
			columns 1 3
	}
	Curve {
		legend "2D sym"
		origin "Trio_U"
		file 2D_sym/test_VIT.son
	Style linespoints
		columns 1 3
	}

	Curve {
		legend "3D perio"
		origin "Trio_U"
		file 3D_perio/test_VIT.son
	Style linespoints
			columns 1 4
	}
	Curve {
		legend "3D sym"
		origin "Trio_U"
		file 3D_sym/test_VIT.son
	Style linespoints
		columns 1 4
	}
		
	Curve {
	        legend "analytique"
		fonction 2
		Style lines
		# TypeLigne 2
		# TypePoints 2
	}
}


}
Chapter {
	Title "Verification invariance rotation et effet porosite"
	Description "Dp/Dy = 0.5.rho.lambda(Re).U2/Dh "
	Description "lamba(Re)=$Re^-3$"

	Description "rho = 5 (solution independante de rho)"
	Description "mu = 4"
	Description "Dh = 2"
	Description "U = 2 "
	Description " "
	Description " P vise en entree : $P = 1/2 rho/Re^3 *U^2/D=1/25=4e-2$"
Table {
        Title "Valeur de la pression en entree"
	nb_columns 1
	label pression
	line {
		legend theorie
		values 4e-2
	}
	line {
	file 3D_sym_inout/test_P.son
	last_line
	nb_columns_file 2
	columns $2
	legend standard
	}
	line {
	file 3D_sym_inout/test_poreux_P.son
	last_line
	nb_columns_file 2
	columns $2
	legend poreux
	}
	line {
	file 3D_sym_inout/test_rotation_P.son
	last_line
	nb_columns_file 2
	columns $2
	legend rotation
	}

}
}
