Parameters
{
    Title "Tests de convergence de l'operateur de diffusion aux elements"
 TestCase Triangles/jdd_1 cas.data
 TestCase Triangles/jdd_2 cas.data
 TestCase Triangles/jdd_3 cas.data
 TestCase Quadrangles/jdd_1 cas.data
 TestCase Quadrangles/jdd_2 cas.data
 TestCase Quadrangles/jdd_3 cas.data
 TestCase Cartesian/jdd_1 cas.data
 TestCase Cartesian/jdd_2 cas.data
 TestCase Cartesian/jdd_3 cas.data
 TestCase Locally_Refined/jdd_1 cas.data
 TestCase Locally_Refined/jdd_2 cas.data
 TestCase Locally_Refined/jdd_3 cas.data
# bloc testcases #

    Author "YG"
    IncludeData 2
    Prerequisite "./extract_convergence"
    Description "Cas test avec cl de Dirichlet, dont la solution analytique est un produit de sinus."
}

