# Thermohydraulique 2D maillage quadrangle Boussinesq #
# PARALLEL OK 8 #
dimension 2
Pb_Thermohydraulique pb
Domaine dom
# BEGIN MESH #
Mailler dom
{
    Pave Cavite
    {
        Origine 0. 0.
        Nombre_de_Noeuds 21 21
        Longueurs 0.01 0.01
    }
    {
        Bord Gauche X = 0.  0. <= Y <= 0.01
        Bord Haut Y = 0.01  0. <= X <= 0.01
        Bord Bas Y = 0.     0. <= X <= 0.01
        Bord Droit X = 0.01 0. <= Y <= 0.01
    }
}

# END MESH #
# BEGIN PARTITION
Partition dom
{
    Partition_tool metis { Nb_parts 2 }
    Larg_joint 1
    zones_name DOM
}
End
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #

VEFPreP1B dis
Read dis { P0 }

Scheme_euler_explicit sch
Read sch
{
    tinit 0.
    tmax 0.2
    dt_min 0.001
    dt_max 0.001
    dt_impr 0.001
    dt_sauv 1
    seuil_statio 1.e-8
}

Associate pb dom
Associate pb sch
Discretize pb dis

Read pb
{
       
fluide_incompressible {
    gravite champ_uniforme 2 0 -9.81

mu Champ_Uniforme 1 1.85e-5
    rho Champ_Uniforme 1 1.17
    lambda Champ_Uniforme 1 0.0262
    Cp Champ_Uniforme 1 1006
    beta_th Champ_Uniforme 1 3.41e-3
}


Navier_Stokes_standard
    {
        solveur_pression GCP { precond ssor { omega 1.5 } seuil 1.e-12 impr }
        convection { Amont }
        diffusion { }
        sources { boussinesq_temperature { T0 5. } }
        initial_conditions {
            vitesse Champ_Uniforme 2 0. 0.
        }
        boundary_conditions {
            Haut paroi_fixe
            Bas paroi_fixe
            Gauche paroi_fixe
            Droit paroi_fixe
        }
    }
    Convection_Diffusion_Temperature
    {
        diffusion { }
        convection { amont }
        boundary_conditions
        {
            Haut paroi_adiabatique
            Bas paroi_adiabatique
            Gauche Paroi_temperature_imposee Champ_Front_Uniforme 1
            10.
            Droit Paroi_temperature_imposee Champ_Front_Uniforme 1 0
        }
        initial_conditions { Temperature Champ_Uniforme 1 5. }
    }

    Post_processing
    {
        Probes
        {
            sonde_pression pression periode 0.01 points 1 0.0045 0.0045
            sonde_vitesse vitesse periode 0.01 points 1 0.0045 0.0045
            sonde_temp temperature periode 0.01 points 1 0.0045 0.0045
            sonde_press pression periode 0.01 segment 6 0. 0.0055 0.01 0.0055
            sonde_vit1 vitesse periode 0.01 segment 6 0.0005 0. 0.0005 0.01
            sonde_temps temperature periode 0.01 segment 6 0. 0.0055 0.01 0.0055

        }
        fields dt_post 0.1
        {
            pression elem
            pression som
            vitesse som
            temperature som
        }
    }
}

Solve pb
End
 