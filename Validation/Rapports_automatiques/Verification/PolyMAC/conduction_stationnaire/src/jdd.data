# PARALLEL OK #
dimension 2
Pb_conduction pb
Domaine dom

# BEGIN MESH #
Lire_MED {
    domain dom
    file mesh.med
}
# END MESH #

# BEGIN PARTITION
Postraiter_domaine
{
    fichier mesh.lata
    format lata
    domaine dom
}

Partition dom
{
    Partition_tool tranche { tranches 2 1 }
    larg_joint 2
    zones_name DOM
}
End
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #

PolyMAC  dis

Schema_euler_implicite sch
Read sch
{
    tinit 0
    dt_max  1
    facsec 1e30
    seuil_statio 1e-8
    solveur implicite
    {
        solveur petsc cli
        {
            -pc_type fieldsplit
            -pc_fieldsplit_0_field b1
            -pc_fieldsplit_1_field b0
            -pc_fieldsplit_type schur
            -ksp_type fgmres
            -ksp_rtol 1e-8
            -fieldsplit_b0_ksp_max_it 10
            -fieldsplit_b1_ksp_max_it 10
        }
    }
}

Associate pb dom
Associate pb sch
Discretize pb dis

Read pb
{

    solide {
        rho Champ_Uniforme 1 1.
        lambda Champ_Uniforme 1 0.25
        Cp Champ_Uniforme 1 1.
    }

    Conduction
    {
        diffusion { }
        sources { Puissance_thermique Champ_Fonc_XYZ dom 1 3*x/2 }
        initial_conditions { temperature Champ_Uniforme 1 0 }
        boundary_conditions
        {
            right paroi_temperature_imposee Champ_Front_Uniforme 1 0
            left  paroi_temperature_imposee Champ_Front_Uniforme 1 0
            up    paroi_adiabatique
            down  paroi_adiabatique
        }
    }

    Post_processing
    {
        Probes
        {
            temperature_x grav temperature periode 1e8 segment 500  0. 0.5 2. 0.5
        }
        format Lata
        fields dt_post 30.
        {
            temperature elem
        }
    }
}

Solve pb
lata_to_other lml NOM_DU_CAS NOM_DU_CAS
End
