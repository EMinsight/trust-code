ORG=`pwd`

# We touch the file .build64 that tells that patch has been applied
touch .build64

fold=$1
[ ! -d $fold ] && echo "No folder named $fold ... we exit" && exit 1
# We do not edit TrioDEC.hxx and .cxx from ICoCo
./Modifie $(find $fold -not -name TrioDEC.?xx)
./Modifie $(find $fold -not -name TrioDEC.?xx)

