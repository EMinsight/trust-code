# Thermohydraulique 2D : Couplage Boussinesq - resolution implicite (Simple) #
# PARALLEL OK #
dimension 2
Pb_Thermohydraulique pb
Domaine dom
# BEGIN MESH #
Mailler dom
{
    Pave Cavite
    {
        Origine 0. 0.
        Nombre_de_Noeuds 11 11
        Longueurs 0.01 0.01
    }
    {
        Bord Gauche X = 0.  0. <= Y <= 0.01
        Bord Haut Y = 0.01  0. <= X <= 0.01
        Bord Bas Y = 0.     0. <= X <= 0.01
        Bord Droit X = 0.01 0. <= Y <= 0.01
    }
}

# END MESH #
# BEGIN PARTITION
Partition dom
{
    Partition_tool tranche { tranches 2 1 }
    Larg_joint 1
    zones_name DOM
}
End
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #

VDF dis

Scheme_euler_implicit sch
Read sch
{
    tinit 0.
    tmax 1000
    dt_min 0.001
    dt_max 100
    dt_impr 0.001
    dt_sauv 100
    seuil_statio 1.e-6
    facsec_max 100
    max_iter_implicite 50

    Solveur
    Simple
    {
        seuil_convergence_implicite 1.e-2
        seuil_convergence_solveur 1.e-14
        relax_pression 0.25
    }
}

Associate pb dom
Associate pb sch
Discretize pb dis

Read pb
{
       
fluide_incompressible {
    gravite champ_uniforme 2 0 -9.81

mu Champ_Uniforme 1 1.85e-5
    rho Champ_Uniforme 1 1.17
    lambda Champ_Uniforme 1 0.0262
    Cp Champ_Uniforme 1 1006
    beta_th Champ_Uniforme 1 3.41e-3
}


Navier_Stokes_standard
    {

        solveur_pression GCP {
            precond_nul
            seuil 1.e-16
            impr
        }
        convection { amont }
        diffusion { }
        sources { boussinesq_temperature { T0 5. } }
        initial_conditions {
            vitesse Champ_Uniforme 2 0. 0.
        }
        boundary_conditions {
            Haut paroi_fixe
            Bas paroi_fixe
            Gauche paroi_fixe
            Droit paroi_fixe
        }
    }
    Convection_Diffusion_Temperature
    {
        diffusion { }
        convection { amont }
        boundary_conditions
        {
            Haut paroi_adiabatique
            Bas paroi_adiabatique
            Gauche paroi_temperature_imposee Champ_Front_Uniforme 1 10.
            Droit  paroi_temperature_imposee Champ_Front_Uniforme 1 0.
        }
        initial_conditions { Temperature Champ_Uniforme 1 5. }
    }

    Post_processing
    {
        Probes
        {
            sonde_V vitesse periode 0.01 segment 10 0.0005 0.005 0.0095 0.005
            sonde_pression pression periode 0.01 points 1 0.006 0.006
            sonde_vitesse vitesse periode 0.01 points 1 0.002 0.002
            sonde_temp temperature periode 0.01 points 1 0.0045 0.0045
        }

        Format lml
        fields dt_post 0.1
        {
            pression elem
            pression som
            vitesse elem
            vitesse som
            temperature elem
            temperature som
            vorticite elem
        }
    }
}

Solve pb
End

 