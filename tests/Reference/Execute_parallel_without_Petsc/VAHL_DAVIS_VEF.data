# Thermohydraulique 2D VEF Boussinesq #
# PARALLEL OK 9 #
# lance_test 1 ecarts #
dimension 2
Pb_Thermohydraulique pb
Domaine dom
# BEGIN MESH #
Mailler dom
{
	Pave Cavite 
		{
		Origine 0. 0.
     		Nombre_de_Noeuds 11 11
		Longueurs 0.01 0.01
		}
		{
		Bord Gauche X = 0.  0. <= Y <= 0.01
		Bord Haut Y = 0.01  0. <= X <= 0.01
		Bord Bas Y = 0.     0. <= X <= 0.01
		Bord Droit X = 0.01 0. <= Y <= 0.01
		}
}
Trianguler dom
Raffiner_isotrope dom
VerifierCoin dom { }
# END MESH #
# BEGIN PARTITION
Partition dom
{
	Partition_tool metis { Nb_parts 2 }
	Larg_joint 2
	zones_name DOMVEF
}
End
END PARTITION #

# BEGIN SCATTER
Scatter DOMVEF.Zones dom
END SCATTER #

VEFPreP1B dis
Read dis { P0 P1 changement_de_base_P1bulle 1 CL_pression_sommet_faible 1 modif_div_face_dirichlet 1 }

Scheme_euler_explicit sch
Read sch
{
	nb_pas_dt_max 50
	tinit 0.
	tmax 1.2
	dt_min 0.00001
	dt_max 1.
	dt_impr 0.0001
	precision_impr 8
	dt_sauv 100
	seuil_statio 1.e-8
	
}

Fluide_Incompressible fluide
Read fluide
{
	mu Champ_Uniforme 1 1.85e-5
	rho Champ_Uniforme 1 1.17
	lambda Champ_Uniforme 1 0.0262 
	Cp Champ_Uniforme 1 1006
	beta_th Champ_Fonc_Fonction 1 temperature 1/(val+273)
}

Champ_Uniforme gravite
Read gravite 2 0 -9.81

Associate fluide gravite
Associate pb dom
Associate pb sch
Associate pb fluide

Discretize pb dis

domaine front
extraire_surface 
  {
    domaine front
    probleme pb
    avec_certains_bords 1 Gauche  
  }
  
Read pb
{
	Navier_Stokes_standard
	{
		solveur_pression GCP { precond ssor { omega 1.5 } seuil 1.e-12 impr }
		convection { generic amont }
		diffusion { }
		sources { boussinesq_temperature { T0 5. } }
		initial_conditions {
			vitesse Champ_Uniforme 2 0. 0.
			}
		boundary_conditions {
			Haut paroi_fixe
			Bas paroi_fixe
			Gauche paroi_fixe
			Droit paroi_fixe
		}
	}
	Convection_Diffusion_Temperature
	{
		diffusion { }
		convection { generic amont }
		boundary_conditions 
		{
			Haut paroi_adiabatique
			Bas paroi_adiabatique
 			Gauche Paroi_temperature_imposee Champ_Front_Uniforme 1 10.
			Droit Paroi_temperature_imposee Champ_Front_Uniforme 1 0
		}
		initial_conditions { Temperature Champ_Uniforme 1 5. }
	}

	Post_processings
	{
	    first_post {
		Probes
		{
			sonde_pression pression periode 0.01 points 1 0.0045 0.0045
			sonde_vitesse vitesse periode 0.01 segment 10 0.0005 0.005 0.0095 0.005
			sonde_vit vitesse periode 0.01 segment 11 0.0005 0. 0.0005 0.01
			sonde_temp temperature periode 0.01 segment 21 0. 0.0055 0.01 0.0055
			
		}
		fichier VAHL_DAVIS_VEF
		Definition_champs 
		{
		  Moyenne_temperature_som_dom Interpolation {
		  	localisation som
			source Moyenne
		  	 {
		    	  t_deb 0. t_fin 0.011
  	            	  source refChamp { Pb_champ pb temperature }   
		   	 }
		  }
		  
		  Moyenne_vitesse_som_dom Interpolation {
		  	localisation som
			source Moyenne
		  	 {
		    	  t_deb 0. t_fin 0.011
  	            	  source refChamp { Pb_champ pb vitesse }   
		   	 }
		  }
		  Moyenne_vitesse_temperature Interpolation {
		  	localisation elem
			source Moyenne {
			    	t_deb 0. t_fin 0.011
			    	source Transformation
			    	{
			    	    	methode formule expression 1 vitesse_natif_dom*temperature_natif_dom
			    	    	sources
			    	    	{
			    	    	    	refChamp { Pb_champ pb vitesse } ,
			    	    	    	refChamp { Pb_champ pb temperature }
			    	    	}
			    	}
		      	}
		  }
		  
		  grad_temp_pression Transformation {
		  	methode formule expression 1 Gradient_temperature_natif_dom*pression_natif_dom
			localisation elem
			sources { 
			  Gradient {
		            source refChamp { Pb_champ pb temperature } } ,
			  refChamp { Pb_champ pb pression }
			
			}
		    }	
		  
		  
		   convection_vitesse_dt Interpolation {
		  	localisation elem
			source Morceau_Equation { type operateur numero 1 option stabilite
			    	source refChamp { Pb_champ pb vitesse }
		   	}
		   }
		   convection_vitesse_dt_som_moyenne Interpolation {
		  	localisation elem
			source Moyenne
		  	 {
		    	  t_deb 0. t_fin 0.011
  	            	  source_reference convection_vitesse_dt   
		   	 }
		   }  
							 
		   pression_min Reduction_0D { methode min 
		   					  source Interpolation 
							  	{ localisation elem 
							  	  source refChamp { Pb_champ pb pression } 
								} 
							}
							
		   pression_max Reduction_0D { methode max
		   					  source Interpolation
							  	{ localisation elem 
		   					  	  source refChamp { Pb_champ pb pression }
								} 
							}
							
		   pression_moy Reduction_0D { methode average 
		   					  source Interpolation
							  { localisation elem 
							    source refChamp { Pb_champ pb pression }
		    					  }
		   					}	
		}
		
		fields dt_post 0.01
		{
			pression elem
			pression som
			vitesse som
			temperature som
			vorticite elem
			Moyenne_vitesse_som_dom
			Moyenne_temperature_som_dom
			convection_vitesse_dt
			convection_vitesse_dt_som_moyenne
			Moyenne_vitesse_temperature
			grad_temp_pression
			gradient_temperature elem
			pression_min
			pression_max
		}
		
		Statistiques dt_post 0.01
		{
		   t_deb 0. t_fin 0.011
		   moyenne pression
		   moyenne vorticite
		   ecart_type vitesse
		   ecart_type temperature
		   ecart_type pression
		   ecart_type vorticite
		}
	     }
	  second_post
	     {
	    	Probes
		  {
		    sonde_dt_diff pbConvection_Diffusion_Temperature_Diffusion_dt periode 0.0000001 points 1 0.0045 0.0045
		    sonde_dt_conv pbConvection_Diffusion_Temperature_Convection_dt periode 0.0000001 points 1 0.0045 0.0045
		  }
		 
		Format lata
		fields dt_post 0.01
		{
			pas_de_temps
		}
		
	     }

	  post_front
          { 
	   Definition_champs 
	    {
	     
             debit_fr Morceau_Equation {
	       type operateur numero 3 option flux_bords				   
	       source refChamp { Pb_champ pb vitesse } 
	      }
	      
	     
	     
	     
	     debit Interpolation {
						   domaine front 
						   localisation elem
		  			           source Morceau_Equation {
	                                            type operateur numero 3 option flux_bords				   
	                                            source refChamp { Pb_champ pb vitesse }
	       					   }
	      				         }

	    				       
	     diffU_X Interpolation {
						   domaine front 
						   localisation elem
		  			           source Morceau_Equation {
	                                            type operateur numero 0 option flux_bords compo 0				   
	                                            source refChamp { Pb_champ pb vitesse }
	       					   }
	      				       }
	     diffU_Y Interpolation {
						   domaine front 
						   localisation elem
		  			           source Morceau_Equation {
	                                            type operateur numero 0 option flux_bords compo 1				   
	                                            source refChamp { Pb_champ pb vitesse }
	       					   }
					      }
	     convU_X Interpolation {
						   domaine front 
						   localisation elem
		  			           source Morceau_Equation {
	                                            type operateur numero 1 option flux_bords compo 0				   
	                                            source refChamp { Pb_champ pb vitesse }
	       					   }
	      				       }
	     convU_Y Interpolation {
						   domaine front 
						   localisation elem
		  			           source Morceau_Equation {
	                                            type operateur numero 1 option flux_bords compo 1				   
	                                            source refChamp { Pb_champ pb vitesse }
	       					   }
	      				       }
					       
					       
	     diffT Interpolation {
						   domaine front 
						   localisation elem
		  			           source Morceau_Equation {
	                                            type operateur numero 0 option flux_bords				   
	                                            source refChamp { Pb_champ pb temperature }
	       					   }
	      				       }
					       			       			       
	     convT Interpolation {
						   domaine front 
						   localisation elem
		  			           source Morceau_Equation {
	                                            type operateur numero 1 option flux_bords			   
	                                            source refChamp { Pb_champ pb temperature }
	       					   }
	      				       } 			       
	     			       

	     
					       
	     debit_sum Reduction_0D { methode sum source_reference debit }				       
	     diffU_X_sum Reduction_0D { methode sum source_reference diffU_X }			       
             diffU_Y_sum Reduction_0D { methode sum source_reference diffU_Y }			       			       			       
	     convU_X_sum Reduction_0D { methode sum source_reference convU_X }			       
             convU_Y_sum Reduction_0D { methode sum source_reference convU_Y }
	     diffT_sum Reduction_0D { methode sum source_reference diffT }			       
             convT_sum Reduction_0D { methode sum source_reference convT }
	     			       			       
	    }
	    
	   Probes
		{
			debit_gauche debit_sum periode 0.0001 numero_elem_sur_maitre 0
			diffuX_gauche diffU_X_sum periode 0.0001 numero_elem_sur_maitre 0
			diffuY_gauche diffU_Y_sum periode 0.0001 numero_elem_sur_maitre 0
			convX_gauche convU_X_sum periode 0.0001 numero_elem_sur_maitre 0
			convY_gauche convU_Y_sum periode 0.0001 numero_elem_sur_maitre 0
			diffT_gauche diffT_sum periode 0.0001 numero_elem_sur_maitre 0
			convT_gauche convT_sum periode 0.0001 numero_elem_sur_maitre 0
		}
		 
	   domaine front	
	   format lata
	   fields dt_post 0.1
	    {
	     vitesse elem
             temperature elem 
	     debit_fr elem 
	     debit
	     diffU_X
	     diffU_Y
	     convU_X
	     convU_Y
	     diffT
	     convT
	     debit_sum
	     diffU_X_sum
	     diffU_Y_sum
	     convU_X_sum
	     convU_Y_sum
	     diffT_sum
	     convT_sum
	    }
	  }
	     
	    
	}
	sauvegarde_simple binaire fichier.sauv
}
Imprimer_flux dom { Gauche }
Imprimer_flux_sum dom { Gauche }
Calculer_moments dom calcul
Solve pb
End

