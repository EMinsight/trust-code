#!/bin/bash

ETUDE_TRUST=`QUELLE_ETUDE.ihm`
export ETUDE_TRUST
cd $ETUDE_TRUST
echo "~" > .ordre_IHM
if [ -f .pcs_Run_sonde ]
then
   kill -15 `cat .pcs_Run_sonde` 1>/dev/null 2>/dev/null
fi
# Le sleep est important car sur certaines machines
# La commande echo dans le .ordre_IHM est trop rapide
# est le fichier n'est pas cree !
sleep 1
