#!/bin/bash

export TRUST_WIZARD_ROOT_DIR=@CMAKE_INSTALL_PREFIX@
export CURVEPLOT_ROOT_DIR=@CURVEPLOT_ROOT_DIR@
export MEDCOUPLING_ROOT_DIR=@MEDCOUPLING_ROOT_DIR@
env=$MEDCOUPLING_ROOT_DIR/env.sh
[ -f $env ] && . $env
MEDCPATH="$MEDCOUPLING_ROOT_DIR/"`echo @SALOME_INSTALL_PYTHON@ | sed "s?/salome??"`
MEDCPATH=@MEDCOUPLING_INSTALL_PYTHON@
export PYTHONPATH=$TRUST_WIZARD_ROOT_DIR/@SALOME_INSTALL_PYTHON@:$CURVEPLOT_ROOT_DIR/@SALOME_INSTALL_PYTHON@:$MEDCPATH:$PYTHONPATH

